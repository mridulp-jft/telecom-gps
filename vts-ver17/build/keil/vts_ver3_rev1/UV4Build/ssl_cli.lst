S#if defined(MBEDTLS_SSL_ALPN)
Sstatic void ssl_write_alpn_ext( mbedtls_ssl_context *ssl,
S                                unsigned char *buf, size_t *olen )
S{
S    unsigned char *p = buf;
S    const unsigned char *end = ssl->out_msg + MBEDTLS_SSL_MAX_CONTENT_LEN;
S    size_t alpnlen = 0;
S    const char **cur;
S
S    *olen = 0;
S
S    if( ssl->conf->alpn_list == NULL )
S    {
S        return;
S    }
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, adding alpn extension" ) );
S
S    for( cur = ssl->conf->alpn_list; *cur != NULL; cur++ )
S        alpnlen += (unsigned char)( strlen( *cur ) & 0xFF ) + 1;
S
S    if( end < p || (size_t)( end - p ) < 6 + alpnlen )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "buffer too small" ) );
S        return;
S    }
S
S    *p++ = (unsigned char)( ( MBEDTLS_TLS_EXT_ALPN >> 8 ) & 0xFF );
S    *p++ = (unsigned char)( ( MBEDTLS_TLS_EXT_ALPN      ) & 0xFF );
S
S    /*
S     * opaque ProtocolName<1..2^8-1>;
S     *
S     * struct {
S     *     ProtocolName protocol_name_list<2..2^16-1>
S     * } ProtocolNameList;
S     */
S
S    /* Skip writing extension and list length for now */
S    p += 4;
S
S    for( cur = ssl->conf->alpn_list; *cur != NULL; cur++ )
S    {
S        *p = (unsigned char)( strlen( *cur ) & 0xFF );
S        memcpy( p + 1, *cur, *p );
S        p += 1 + *p;
S    }
S
S    *olen = p - buf;
S
S    /* List length = olen - 2 (ext_type) - 2 (ext_len) - 2 (list_len) */
S    buf[4] = (unsigned char)( ( ( *olen - 6 ) >> 8 ) & 0xFF );
S    buf[5] = (unsigned char)( ( ( *olen - 6 )      ) & 0xFF );
S
S    /* Extension length = olen - 2 (ext_type) - 2 (ext_len) */
S    buf[2] = (unsigned char)( ( ( *olen - 4 ) >> 8 ) & 0xFF );
S    buf[3] = (unsigned char)( ( ( *olen - 4 )      ) & 0xFF );
S}
