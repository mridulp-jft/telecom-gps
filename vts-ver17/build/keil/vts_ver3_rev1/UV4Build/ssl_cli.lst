S    *p++ = (unsigned char)( ( MBEDTLS_TLS_EXT_SUPPORTED_ELLIPTIC_CURVES      ) & 0xFF );
S
S    *p++ = (unsigned char)( ( ( elliptic_curve_len + 2 ) >> 8 ) & 0xFF );
S    *p++ = (unsigned char)( ( ( elliptic_curve_len + 2 )      ) & 0xFF );
S
S    *p++ = (unsigned char)( ( ( elliptic_curve_len     ) >> 8 ) & 0xFF );
S    *p++ = (unsigned char)( ( ( elliptic_curve_len     )      ) & 0xFF );
