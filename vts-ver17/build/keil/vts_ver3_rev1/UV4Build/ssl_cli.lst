S#endif /* MBEDTLS_KEY_EXCHANGE_DHE_RSA_ENABLED ||
S          MBEDTLS_KEY_EXCHANGE_DHE_PSK_ENABLED */
S
S#if defined(MBEDTLS_KEY_EXCHANGE_ECDHE_RSA_ENABLED) ||                     \
