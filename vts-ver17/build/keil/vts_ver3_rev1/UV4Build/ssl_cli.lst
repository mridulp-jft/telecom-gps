S/*
S * Only if we handle at least one key exchange that needs signatures.
S */
S#if defined(MBEDTLS_SSL_PROTO_TLS1_2) && \
S    defined(MBEDTLS_KEY_EXCHANGE__WITH_CERT__ENABLED)
X#if defined(MBEDTLS_SSL_PROTO_TLS1_2) &&     defined(MBEDTLS_KEY_EXCHANGE__WITH_CERT__ENABLED)
Sstatic void ssl_write_signature_algorithms_ext( mbedtls_ssl_context *ssl,
S                                                unsigned char *buf,
S                                                size_t *olen )
S{
S    unsigned char *p = buf;
S    const unsigned char *end = ssl->out_msg + MBEDTLS_SSL_MAX_CONTENT_LEN;
S    size_t sig_alg_len = 0;
S    const int *md;
S#if defined(MBEDTLS_RSA_C) || defined(MBEDTLS_ECDSA_C)
S    unsigned char *sig_alg_list = buf + 6;
S#endif
S
S    *olen = 0;
S
S    if( ssl->conf->max_minor_ver != MBEDTLS_SSL_MINOR_VERSION_3 )
S        return;
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, adding signature_algorithms extension" ) );
S
S    for( md = ssl->conf->sig_hashes; *md != MBEDTLS_MD_NONE; md++ )
S    {
S#if defined(MBEDTLS_ECDSA_C)
S        sig_alg_len += 2;
S#endif
S#if defined(MBEDTLS_RSA_C)
S        sig_alg_len += 2;
S#endif
S    }
S
S    if( end < p || (size_t)( end - p ) < sig_alg_len + 6 )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "buffer too small" ) );
S        return;
S    }
S
S    /*
S     * Prepare signature_algorithms extension (TLS 1.2)
S     */
S    sig_alg_len = 0;
S
S    for( md = ssl->conf->sig_hashes; *md != MBEDTLS_MD_NONE; md++ )
S    {
S#if defined(MBEDTLS_ECDSA_C)
S        sig_alg_list[sig_alg_len++] = mbedtls_ssl_hash_from_md_alg( *md );
S        sig_alg_list[sig_alg_len++] = MBEDTLS_SSL_SIG_ECDSA;
S#endif
S#if defined(MBEDTLS_RSA_C)
S        sig_alg_list[sig_alg_len++] = mbedtls_ssl_hash_from_md_alg( *md );
S        sig_alg_list[sig_alg_len++] = MBEDTLS_SSL_SIG_RSA;
S#endif
S    }
S
S    /*
S     * enum {
S     *     none(0), md5(1), sha1(2), sha224(3), sha256(4), sha384(5),
S     *     sha512(6), (255)
S     * } HashAlgorithm;
S     *
S     * enum { anonymous(0), rsa(1), dsa(2), ecdsa(3), (255) }
S     *   SignatureAlgorithm;
S     *
S     * struct {
S     *     HashAlgorithm hash;
S     *     SignatureAlgorithm signature;
S     * } SignatureAndHashAlgorithm;
S     *
S     * SignatureAndHashAlgorithm
S     *   supported_signature_algorithms<2..2^16-2>;
S     */
S    *p++ = (unsigned char)( ( MBEDTLS_TLS_EXT_SIG_ALG >> 8 ) & 0xFF );
S    *p++ = (unsigned char)( ( MBEDTLS_TLS_EXT_SIG_ALG      ) & 0xFF );
S
S    *p++ = (unsigned char)( ( ( sig_alg_len + 2 ) >> 8 ) & 0xFF );
S    *p++ = (unsigned char)( ( ( sig_alg_len + 2 )      ) & 0xFF );
S
S    *p++ = (unsigned char)( ( sig_alg_len >> 8 ) & 0xFF );
S    *p++ = (unsigned char)( ( sig_alg_len      ) & 0xFF );
S
S    *olen = 6 + sig_alg_len;
S}
S#endif /* MBEDTLS_SSL_PROTO_TLS1_2 &&
S          MBEDTLS_KEY_EXCHANGE__WITH_CERT__ENABLED */
