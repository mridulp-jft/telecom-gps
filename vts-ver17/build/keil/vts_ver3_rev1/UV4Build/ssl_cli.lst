S#endif
S
S    memcpy( ssl->handshake->randbytes + 32, buf + 2, 32 );
S
S    n = buf[34];
S
S    MBEDTLS_SSL_DEBUG_BUF( 3,   "server hello, random bytes", buf + 2, 32 );
S
S    if( n > 32 )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad server hello message" ) );
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S    }
S
S    if( ssl->in_hslen > mbedtls_ssl_hs_hdr_len( ssl ) + 39 + n )
S    {
S        ext_len = ( ( buf[38 + n] <<  8 )
S                  | ( buf[39 + n]       ) );
S
S        if( ( ext_len > 0 && ext_len < 4 ) ||
S            ssl->in_hslen != mbedtls_ssl_hs_hdr_len( ssl ) + 40 + n + ext_len )
S        {
S            MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad server hello message" ) );
S            return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S        }
S    }
S    else if( ssl->in_hslen == mbedtls_ssl_hs_hdr_len( ssl ) + 38 + n )
S    {
S        ext_len = 0;
S    }
S    else
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad server hello message" ) );
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
