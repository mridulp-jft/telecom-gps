Sstatic int ssl_parse_server_hello( mbedtls_ssl_context *ssl )
S{
S    int ret, i;
S    size_t n;
S    size_t ext_len;
S    unsigned char *buf, *ext;
S    unsigned char comp;
S#if defined(MBEDTLS_ZLIB_SUPPORT)
S    int accept_comp;
S#endif
S#if defined(MBEDTLS_SSL_RENEGOTIATION)
S    int renegotiation_info_seen = 0;
S#endif
S    int handshake_failure = 0;
S    const mbedtls_ssl_ciphersuite_t *suite_info;
S#if defined(MBEDTLS_DEBUG_C)
S    uint32_t t;
S#endif
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "=> parse server hello" ) );
S
S    buf = ssl->in_msg;
S
S    if( ( ret = mbedtls_ssl_read_record( ssl ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "mbedtls_ssl_read_record", ret );
S        return( ret );
S    }
S
S    if( ssl->in_msgtype != MBEDTLS_SSL_MSG_HANDSHAKE )
S    {
S#if defined(MBEDTLS_SSL_RENEGOTIATION)
S        if( ssl->renego_status == MBEDTLS_SSL_RENEGOTIATION_IN_PROGRESS )
S        {
S            ssl->renego_records_seen++;
S
S            if( ssl->conf->renego_max_records >= 0 &&
S                ssl->renego_records_seen > ssl->conf->renego_max_records )
S            {
S                MBEDTLS_SSL_DEBUG_MSG( 1, ( "renegotiation requested, "
S                                    "but not honored by server" ) );
S                return( MBEDTLS_ERR_SSL_UNEXPECTED_MESSAGE );
S            }
S
S            MBEDTLS_SSL_DEBUG_MSG( 1, ( "non-handshake message during renego" ) );
S            return( MBEDTLS_ERR_SSL_WAITING_SERVER_HELLO_RENEGO );
S        }
S#endif /* MBEDTLS_SSL_RENEGOTIATION */
S
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad server hello message" ) );
S        return( MBEDTLS_ERR_SSL_UNEXPECTED_MESSAGE );
S    }
S
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
S    if( ssl->conf->transport == MBEDTLS_SSL_TRANSPORT_DATAGRAM )
S    {
S        if( buf[0] == MBEDTLS_SSL_HS_HELLO_VERIFY_REQUEST )
S        {
S            MBEDTLS_SSL_DEBUG_MSG( 2, ( "received hello verify request" ) );
S            MBEDTLS_SSL_DEBUG_MSG( 2, ( "<= parse server hello" ) );
S            return( ssl_parse_hello_verify_request( ssl ) );
S        }
S        else
S        {
S            /* We made it through the verification process */
S            mbedtls_free( ssl->handshake->verify_cookie );
S            ssl->handshake->verify_cookie = NULL;
S            ssl->handshake->verify_cookie_len = 0;
S        }
S    }
S#endif /* MBEDTLS_SSL_PROTO_DTLS */
S
S    if( ssl->in_hslen < 38 + mbedtls_ssl_hs_hdr_len( ssl ) ||
S        buf[0] != MBEDTLS_SSL_HS_SERVER_HELLO )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad server hello message" ) );
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S    }
S
S    /*
S     *  0   .  1    server_version
S     *  2   . 33    random (maybe including 4 bytes of Unix time)
S     * 34   . 34    session_id length = n
S     * 35   . 34+n  session_id
S     * 35+n . 36+n  cipher_suite
S     * 37+n . 37+n  compression_method
S     *
S     * 38+n . 39+n  extensions length (optional)
S     * 40+n .  ..   extensions
S     */
S    buf += mbedtls_ssl_hs_hdr_len( ssl );
S
S    MBEDTLS_SSL_DEBUG_BUF( 3, "server hello, version", buf + 0, 2 );
S    mbedtls_ssl_read_version( &ssl->major_ver, &ssl->minor_ver,
S                      ssl->conf->transport, buf + 0 );
S
S    if( ssl->major_ver < ssl->conf->min_major_ver ||
S        ssl->minor_ver < ssl->conf->min_minor_ver ||
S        ssl->major_ver > ssl->conf->max_major_ver ||
S        ssl->minor_ver > ssl->conf->max_minor_ver )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "server version out of bounds - "
S                            " min: [%d:%d], server: [%d:%d], max: [%d:%d]",
S                            ssl->conf->min_major_ver, ssl->conf->min_minor_ver,
S                            ssl->major_ver, ssl->minor_ver,
S                            ssl->conf->max_major_ver, ssl->conf->max_minor_ver ) );
S
S        mbedtls_ssl_send_alert_message( ssl, MBEDTLS_SSL_ALERT_LEVEL_FATAL,
S                                     MBEDTLS_SSL_ALERT_MSG_PROTOCOL_VERSION );
S
S        return( MBEDTLS_ERR_SSL_BAD_HS_PROTOCOL_VERSION );
S    }
S
S#if defined(MBEDTLS_DEBUG_C)
S    t = ( (uint32_t) buf[2] << 24 )
S      | ( (uint32_t) buf[3] << 16 )
S      | ( (uint32_t) buf[4] <<  8 )
S      | ( (uint32_t) buf[5]       );
