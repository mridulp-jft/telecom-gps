Sstatic int ssl_write_encrypted_pms( mbedtls_ssl_context *ssl,
S                                    size_t offset, size_t *olen,
S                                    size_t pms_offset )
S{
S    int ret;
S    size_t len_bytes = ssl->minor_ver == MBEDTLS_SSL_MINOR_VERSION_0 ? 0 : 2;
S    unsigned char *p = ssl->handshake->premaster + pms_offset;
