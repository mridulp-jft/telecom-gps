S/*
S * Generate random bytes for ClientHello
S */
Sstatic int ssl_generate_random( mbedtls_ssl_context *ssl )
S{
S    int ret;
S    unsigned char *p = ssl->handshake->randbytes;
S#if defined(MBEDTLS_HAVE_TIME)
S    time_t t;
S#endif
S
S    /*
S     * When responding to a verify request, MUST reuse random (RFC 6347 4.2.1)
S     */
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
S    if( ssl->conf->transport == MBEDTLS_SSL_TRANSPORT_DATAGRAM &&
S        ssl->handshake->verify_cookie != NULL )
S    {
S        return( 0 );
S    }
S#endif
S
S#if defined(MBEDTLS_HAVE_TIME)
S    t = time( NULL );
S    *p++ = (unsigned char)( t >> 24 );
S    *p++ = (unsigned char)( t >> 16 );
S    *p++ = (unsigned char)( t >>  8 );
S    *p++ = (unsigned char)( t       );
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, current time: %lu", t ) );
S#else
S    if( ( ret = ssl->conf->f_rng( ssl->conf->p_rng, p, 4 ) ) != 0 )
S        return( ret );
S
S    p += 4;
S#endif /* MBEDTLS_HAVE_TIME */
S
S    if( ( ret = ssl->conf->f_rng( ssl->conf->p_rng, p, 28 ) ) != 0 )
S        return( ret );
S
S    return( 0 );
S}
