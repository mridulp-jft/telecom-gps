S#else
Sstatic int ssl_write_certificate_verify( mbedtls_ssl_context *ssl )
S{
S    int ret = MBEDTLS_ERR_SSL_FEATURE_UNAVAILABLE;
S    const mbedtls_ssl_ciphersuite_t *ciphersuite_info = ssl->transform_negotiate->ciphersuite_info;
S    size_t n = 0, offset = 0;
S    unsigned char hash[48];
S    unsigned char *hash_start = hash;
S    mbedtls_md_type_t md_alg = MBEDTLS_MD_NONE;
S    unsigned int hashlen;
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "=> write certificate verify" ) );
S
S    if( ( ret = mbedtls_ssl_derive_keys( ssl ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "mbedtls_ssl_derive_keys", ret );
S        return( ret );
S    }
S
S    if( ciphersuite_info->key_exchange == MBEDTLS_KEY_EXCHANGE_PSK ||
S        ciphersuite_info->key_exchange == MBEDTLS_KEY_EXCHANGE_RSA_PSK ||
S        ciphersuite_info->key_exchange == MBEDTLS_KEY_EXCHANGE_ECDHE_PSK ||
S        ciphersuite_info->key_exchange == MBEDTLS_KEY_EXCHANGE_DHE_PSK ||
S        ciphersuite_info->key_exchange == MBEDTLS_KEY_EXCHANGE_ECJPAKE )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 2, ( "<= skip write certificate verify" ) );
S        ssl->state++;
S        return( 0 );
S    }
S
S    if( ssl->client_auth == 0 || mbedtls_ssl_own_cert( ssl ) == NULL )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 2, ( "<= skip write certificate verify" ) );
S        ssl->state++;
S        return( 0 );
S    }
S
S    if( mbedtls_ssl_own_key( ssl ) == NULL )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "got no private key for certificate" ) );
S        return( MBEDTLS_ERR_SSL_PRIVATE_KEY_REQUIRED );
S    }
S
S    /*
S     * Make an RSA signature of the handshake digests
S     */
S    ssl->handshake->calc_verify( ssl, hash );
S
S#if defined(MBEDTLS_SSL_PROTO_SSL3) || defined(MBEDTLS_SSL_PROTO_TLS1) || \
S    defined(MBEDTLS_SSL_PROTO_TLS1_1)
X#if defined(MBEDTLS_SSL_PROTO_SSL3) || defined(MBEDTLS_SSL_PROTO_TLS1) ||     defined(MBEDTLS_SSL_PROTO_TLS1_1)
S    if( ssl->minor_ver != MBEDTLS_SSL_MINOR_VERSION_3 )
S    {
S        /*
S         * digitally-signed struct {
S         *     opaque md5_hash[16];
S         *     opaque sha_hash[20];
S         * };
S         *
S         * md5_hash
S         *     MD5(handshake_messages);
S         *
S         * sha_hash
S         *     SHA(handshake_messages);
S         */
S        hashlen = 36;
S        md_alg = MBEDTLS_MD_NONE;
S
S        /*
S         * For ECDSA, default hash is SHA-1 only
S         */
S        if( mbedtls_pk_can_do( mbedtls_ssl_own_key( ssl ), MBEDTLS_PK_ECDSA ) )
S        {
S            hash_start += 16;
S            hashlen -= 16;
S            md_alg = MBEDTLS_MD_SHA1;
S        }
S    }
S    else
S#endif /* MBEDTLS_SSL_PROTO_SSL3 || MBEDTLS_SSL_PROTO_TLS1 || \
S          MBEDTLS_SSL_PROTO_TLS1_1 */
X#endif  
S#if defined(MBEDTLS_SSL_PROTO_TLS1_2)
S    if( ssl->minor_ver == MBEDTLS_SSL_MINOR_VERSION_3 )
S    {
S        /*
S         * digitally-signed struct {
S         *     opaque handshake_messages[handshake_messages_length];
S         * };
S         *
S         * Taking shortcut here. We assume that the server always allows the
S         * PRF Hash function and has sent it in the allowed signature
S         * algorithms list received in the Certificate Request message.
S         *
S         * Until we encounter a server that does not, we will take this
S         * shortcut.
S         *
S         * Reason: Otherwise we should have running hashes for SHA512 and SHA224
S         *         in order to satisfy 'weird' needs from the server side.
S         */
S        if( ssl->transform_negotiate->ciphersuite_info->mac ==
S            MBEDTLS_MD_SHA384 )
S        {
S            md_alg = MBEDTLS_MD_SHA384;
S            ssl->out_msg[4] = MBEDTLS_SSL_HASH_SHA384;
S        }
S        else
S        {
S            md_alg = MBEDTLS_MD_SHA256;
S            ssl->out_msg[4] = MBEDTLS_SSL_HASH_SHA256;
S        }
S        ssl->out_msg[5] = mbedtls_ssl_sig_from_pk( mbedtls_ssl_own_key( ssl ) );
S
S        /* Info from md_alg will be used instead */
S        hashlen = 0;
S        offset = 2;
S    }
S    else
S#endif /* MBEDTLS_SSL_PROTO_TLS1_2 */
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "should never happen" ) );
S        return( MBEDTLS_ERR_SSL_INTERNAL_ERROR );
S    }
S
S    if( ( ret = mbedtls_pk_sign( mbedtls_ssl_own_key( ssl ), md_alg, hash_start, hashlen,
S                         ssl->out_msg + 6 + offset, &n,
S                         ssl->conf->f_rng, ssl->conf->p_rng ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "mbedtls_pk_sign", ret );
S        return( ret );
S    }
S
S    ssl->out_msg[4 + offset] = (unsigned char)( n >> 8 );
S    ssl->out_msg[5 + offset] = (unsigned char)( n      );
S
S    ssl->out_msglen  = 6 + n + offset;
S    ssl->out_msgtype = MBEDTLS_SSL_MSG_HANDSHAKE;
S    ssl->out_msg[0]  = MBEDTLS_SSL_HS_CERTIFICATE_VERIFY;
S
S    ssl->state++;
S
S    if( ( ret = mbedtls_ssl_write_record( ssl ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "mbedtls_ssl_write_record", ret );
S        return( ret );
S    }
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "<= write certificate verify" ) );
S
S    return( ret );
S}
S#endif /* !MBEDTLS_KEY_EXCHANGE_RSA_ENABLED &&
S          !MBEDTLS_KEY_EXCHANGE_DHE_RSA_ENABLED &&
S          !MBEDTLS_KEY_EXCHANGE_ECDHE_RSA_ENABLED */
S
S#if defined(MBEDTLS_SSL_SESSION_TICKETS)
Sstatic int ssl_parse_new_session_ticket( mbedtls_ssl_context *ssl )
S{
S    int ret;
S    uint32_t lifetime;
S    size_t ticket_len;
S    unsigned char *ticket;
S    const unsigned char *msg;
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "=> parse new session ticket" ) );
S
S    if( ( ret = mbedtls_ssl_read_record( ssl ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "mbedtls_ssl_read_record", ret );
S        return( ret );
S    }
S
S    if( ssl->in_msgtype != MBEDTLS_SSL_MSG_HANDSHAKE )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad new session ticket message" ) );
S        return( MBEDTLS_ERR_SSL_UNEXPECTED_MESSAGE );
S    }
S
S    /*
S     * struct {
S     *     uint32 ticket_lifetime_hint;
S     *     opaque ticket<0..2^16-1>;
S     * } NewSessionTicket;
S     *
S     * 0  .  3   ticket_lifetime_hint
S     * 4  .  5   ticket_len (n)
S     * 6  .  5+n ticket content
S     */
S    if( ssl->in_msg[0] != MBEDTLS_SSL_HS_NEW_SESSION_TICKET ||
S        ssl->in_hslen < 6 + mbedtls_ssl_hs_hdr_len( ssl ) )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad new session ticket message" ) );
S        return( MBEDTLS_ERR_SSL_BAD_HS_NEW_SESSION_TICKET );
S    }
S
S    msg = ssl->in_msg + mbedtls_ssl_hs_hdr_len( ssl );
S
S    lifetime = ( msg[0] << 24 ) | ( msg[1] << 16 ) |
S               ( msg[2] <<  8 ) | ( msg[3]       );
S
S    ticket_len = ( msg[4] << 8 ) | ( msg[5] );
S
S    if( ticket_len + 6 + mbedtls_ssl_hs_hdr_len( ssl ) != ssl->in_hslen )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad new session ticket message" ) );
S        return( MBEDTLS_ERR_SSL_BAD_HS_NEW_SESSION_TICKET );
S    }
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "ticket length: %d", ticket_len ) );
S
S    /* We're not waiting for a NewSessionTicket message any more */
S    ssl->handshake->new_session_ticket = 0;
S    ssl->state = MBEDTLS_SSL_SERVER_CHANGE_CIPHER_SPEC;
S
S    /*
S     * Zero-length ticket means the server changed his mind and doesn't want
S     * to send a ticket after all, so just forget it
S     */
S    if( ticket_len == 0 )
S        return( 0 );
S
S    mbedtls_zeroize( ssl->session_negotiate->ticket,
S                      ssl->session_negotiate->ticket_len );
S    mbedtls_free( ssl->session_negotiate->ticket );
S    ssl->session_negotiate->ticket = NULL;
S    ssl->session_negotiate->ticket_len = 0;
S
S    if( ( ticket = mbedtls_calloc( 1, ticket_len ) ) == NULL )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "ticket alloc failed" ) );
S        return( MBEDTLS_ERR_SSL_ALLOC_FAILED );
S    }
S
S    memcpy( ticket, msg + 6, ticket_len );
S
S    ssl->session_negotiate->ticket = ticket;
S    ssl->session_negotiate->ticket_len = ticket_len;
S    ssl->session_negotiate->ticket_lifetime = lifetime;
S
S    /*
S     * RFC 5077 section 3.4:
S     * "If the client receives a session ticket from the server, then it
S     * discards any Session ID that was sent in the ServerHello."
S     */
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "ticket in use, discarding session id" ) );
S    ssl->session_negotiate->id_len = 0;
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "<= parse new session ticket" ) );
S
S    return( 0 );
S}
S#endif /* MBEDTLS_SSL_SESSION_TICKETS */
S
S/*
S * SSL handshake -- client side -- single step
S */
Sint mbedtls_ssl_handshake_client_step( mbedtls_ssl_context *ssl )
S{
S    int ret = 0;
S
S    if( ssl->state == MBEDTLS_SSL_HANDSHAKE_OVER || ssl->handshake == NULL )
S        return( MBEDTLS_ERR_SSL_BAD_INPUT_DATA );
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "client state: %d", ssl->state ) );
S
S    if( ( ret = mbedtls_ssl_flush_output( ssl ) ) != 0 )
S        return( ret );
S
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
S    if( ssl->conf->transport == MBEDTLS_SSL_TRANSPORT_DATAGRAM &&
S        ssl->handshake->retransmit_state == MBEDTLS_SSL_RETRANS_SENDING )
S    {
S        if( ( ret = mbedtls_ssl_resend( ssl ) ) != 0 )
S            return( ret );
S    }
S#endif
S
S    /* Change state now, so that it is right in mbedtls_ssl_read_record(), used
S     * by DTLS for dropping out-of-sequence ChangeCipherSpec records */
S#if defined(MBEDTLS_SSL_SESSION_TICKETS)
S    if( ssl->state == MBEDTLS_SSL_SERVER_CHANGE_CIPHER_SPEC &&
S        ssl->handshake->new_session_ticket != 0 )
S    {
S        ssl->state = MBEDTLS_SSL_SERVER_NEW_SESSION_TICKET;
S    }
S#endif
S
S    switch( ssl->state )
S    {
S        case MBEDTLS_SSL_HELLO_REQUEST:
S            ssl->state = MBEDTLS_SSL_CLIENT_HELLO;
S            break;
S
S       /*
S        *  ==>   ClientHello
S        */
S       case MBEDTLS_SSL_CLIENT_HELLO:
S           ret = ssl_write_client_hello( ssl );
S           break;
S
S       /*
S        *  <==   ServerHello
S        *        Certificate
S        *      ( ServerKeyExchange  )
S        *      ( CertificateRequest )
S        *        ServerHelloDone
S        */
S       case MBEDTLS_SSL_SERVER_HELLO:
S           ret = ssl_parse_server_hello( ssl );
S           break;
S
S       case MBEDTLS_SSL_SERVER_CERTIFICATE:
S           ret = mbedtls_ssl_parse_certificate( ssl );
S           break;
S
S       case MBEDTLS_SSL_SERVER_KEY_EXCHANGE:
S           ret = ssl_parse_server_key_exchange( ssl );
S           break;
S
S       case MBEDTLS_SSL_CERTIFICATE_REQUEST:
S           ret = ssl_parse_certificate_request( ssl );
S           break;
S
S       case MBEDTLS_SSL_SERVER_HELLO_DONE:
S           ret = ssl_parse_server_hello_done( ssl );
S           break;
S
S       /*
S        *  ==> ( Certificate/Alert  )
S        *        ClientKeyExchange
S        *      ( CertificateVerify  )
S        *        ChangeCipherSpec
S        *        Finished
S        */
S       case MBEDTLS_SSL_CLIENT_CERTIFICATE:
S           ret = mbedtls_ssl_write_certificate( ssl );
S           break;
S
S       case MBEDTLS_SSL_CLIENT_KEY_EXCHANGE:
S           ret = ssl_write_client_key_exchange( ssl );
S           break;
S
S       case MBEDTLS_SSL_CERTIFICATE_VERIFY:
S           ret = ssl_write_certificate_verify( ssl );
S           break;
S
S       case MBEDTLS_SSL_CLIENT_CHANGE_CIPHER_SPEC:
S           ret = mbedtls_ssl_write_change_cipher_spec( ssl );
S           break;
S
S       case MBEDTLS_SSL_CLIENT_FINISHED:
S           ret = mbedtls_ssl_write_finished( ssl );
S           break;
S
S       /*
S        *  <==   ( NewSessionTicket )
S        *        ChangeCipherSpec
S        *        Finished
S        */
S#if defined(MBEDTLS_SSL_SESSION_TICKETS)
S       case MBEDTLS_SSL_SERVER_NEW_SESSION_TICKET:
S           ret = ssl_parse_new_session_ticket( ssl );
S           break;
S#endif
S
S       case MBEDTLS_SSL_SERVER_CHANGE_CIPHER_SPEC:
S           ret = mbedtls_ssl_parse_change_cipher_spec( ssl );
S           break;
S
S       case MBEDTLS_SSL_SERVER_FINISHED:
S           ret = mbedtls_ssl_parse_finished( ssl );
S           break;
S
S       case MBEDTLS_SSL_FLUSH_BUFFERS:
S           MBEDTLS_SSL_DEBUG_MSG( 2, ( "handshake: done" ) );
S           ssl->state = MBEDTLS_SSL_HANDSHAKE_WRAPUP;
S           break;
S
S       case MBEDTLS_SSL_HANDSHAKE_WRAPUP:
S           mbedtls_ssl_handshake_wrapup( ssl );
S           break;
S
S       default:
S           MBEDTLS_SSL_DEBUG_MSG( 1, ( "invalid state %d", ssl->state ) );
S           return( MBEDTLS_ERR_SSL_BAD_INPUT_DATA );
S   }
S
S    return( ret );
S}
