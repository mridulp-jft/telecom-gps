S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "DHM prime too short: %d < %d",
S                                    ssl->handshake->dhm_ctx.len * 8,
S                                    ssl->conf->dhm_min_bitlen ) );
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_KEY_EXCHANGE );
