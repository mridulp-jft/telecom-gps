S#if defined(MBEDTLS_ZLIB_SUPPORT)
S    /* See comments in ssl_write_client_hello() */
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
S    if( ssl->conf->transport == MBEDTLS_SSL_TRANSPORT_DATAGRAM )
S        accept_comp = 0;
S    else
S#endif
S        accept_comp = 1;
S
S    if( comp != MBEDTLS_SSL_COMPRESS_NULL &&
S        ( comp != MBEDTLS_SSL_COMPRESS_DEFLATE || accept_comp == 0 ) )
S#else /* MBEDTLS_ZLIB_SUPPORT */
S    if( comp != MBEDTLS_SSL_COMPRESS_NULL )
S#endif/* MBEDTLS_ZLIB_SUPPORT */
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "server hello, bad compression: %d", comp ) );
S        return( MBEDTLS_ERR_SSL_FEATURE_UNAVAILABLE );
S    }
S
S    /*
S     * Initialize update checksum functions
S     */
S    ssl->transform_negotiate->ciphersuite_info = mbedtls_ssl_ciphersuite_from_id( i );
S
S    if( ssl->transform_negotiate->ciphersuite_info == NULL )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "ciphersuite info for %04x not found", i ) );
S        return( MBEDTLS_ERR_SSL_BAD_INPUT_DATA );
S    }
S
S    mbedtls_ssl_optimize_checksum( ssl, ssl->transform_negotiate->ciphersuite_info );
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "server hello, session id len.: %d", n ) );
S    MBEDTLS_SSL_DEBUG_BUF( 3,   "server hello, session id", buf + 35, n );
S
S    /*
S     * Check if the session can be resumed
S     */
