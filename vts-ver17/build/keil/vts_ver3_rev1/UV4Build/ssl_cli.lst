S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "buffer too small for encrypted pms" ) );
S        return( MBEDTLS_ERR_SSL_BUFFER_TOO_SMALL );
S    }
S
S    /*
S     * Generate (part of) the pre-master as
S     *  struct {
S     *      ProtocolVersion client_version;
S     *      opaque random[46];
S     *  } PreMasterSecret;
S     */
S    mbedtls_ssl_write_version( ssl->conf->max_major_ver, ssl->conf->max_minor_ver,
S                       ssl->conf->transport, p );
S
S    if( ( ret = ssl->conf->f_rng( ssl->conf->p_rng, p + 2, 46 ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "f_rng", ret );
S        return( ret );
S    }
S
S    ssl->handshake->pmslen = 48;
S
S    if( ssl->session_negotiate->peer_cert == NULL )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 2, ( "certificate required" ) );
S        return( MBEDTLS_ERR_SSL_UNEXPECTED_MESSAGE );
S    }
