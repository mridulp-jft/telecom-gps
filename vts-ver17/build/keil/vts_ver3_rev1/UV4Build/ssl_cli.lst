Sstatic int ssl_write_client_hello( mbedtls_ssl_context *ssl )
S{
S    int ret;
S    size_t i, n, olen, ext_len = 0;
S    unsigned char *buf;
S    unsigned char *p, *q;
S    unsigned char offer_compress;
S    const int *ciphersuites;
S    const mbedtls_ssl_ciphersuite_t *ciphersuite_info;
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "=> write client hello" ) );
S
S    if( ssl->conf->f_rng == NULL )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "no RNG provided") );
S        return( MBEDTLS_ERR_SSL_NO_RNG );
S    }
S
S#if defined(MBEDTLS_SSL_RENEGOTIATION)
S    if( ssl->renego_status == MBEDTLS_SSL_INITIAL_HANDSHAKE )
S#endif
S    {
S        ssl->major_ver = ssl->conf->min_major_ver;
S        ssl->minor_ver = ssl->conf->min_minor_ver;
S    }
S
S    if( ssl->conf->max_major_ver == 0 )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "configured max major version is invalid, "
S                            "consider using mbedtls_ssl_config_defaults()" ) );
S        return( MBEDTLS_ERR_SSL_BAD_INPUT_DATA );
S    }
S
S    /*
S     *     0  .   0   handshake type
S     *     1  .   3   handshake length
S     *     4  .   5   highest version supported
S     *     6  .   9   current UNIX time
S     *    10  .  37   random bytes
S     */
S    buf = ssl->out_msg;
S    p = buf + 4;
S
S    mbedtls_ssl_write_version( ssl->conf->max_major_ver, ssl->conf->max_minor_ver,
S                       ssl->conf->transport, p );
S    p += 2;
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, max version: [%d:%d]",
S                   buf[4], buf[5] ) );
S
S    if( ( ret = ssl_generate_random( ssl ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "ssl_generate_random", ret );
S        return( ret );
S    }
S
S    memcpy( p, ssl->handshake->randbytes, 32 );
S    MBEDTLS_SSL_DEBUG_BUF( 3, "client hello, random bytes", p, 32 );
S    p += 32;
S
S    /*
S     *    38  .  38   session id length
S     *    39  . 39+n  session id
S     *   39+n . 39+n  DTLS only: cookie length (1 byte)
S     *   40+n .  ..   DTSL only: cookie
S     *   ..   . ..    ciphersuitelist length (2 bytes)
S     *   ..   . ..    ciphersuitelist
S     *   ..   . ..    compression methods length (1 byte)
S     *   ..   . ..    compression methods
S     *   ..   . ..    extensions length (2 bytes)
S     *   ..   . ..    extensions
S     */
S    n = ssl->session_negotiate->id_len;
S
S    if( n < 16 || n > 32 ||
S#if defined(MBEDTLS_SSL_RENEGOTIATION)
S        ssl->renego_status != MBEDTLS_SSL_INITIAL_HANDSHAKE ||
S#endif
S        ssl->handshake->resume == 0 )
S    {
S        n = 0;
S    }
S
S#if defined(MBEDTLS_SSL_SESSION_TICKETS)
S    /*
S     * RFC 5077 section 3.4: "When presenting a ticket, the client MAY
S     * generate and include a Session ID in the TLS ClientHello."
S     */
S#if defined(MBEDTLS_SSL_RENEGOTIATION)
S    if( ssl->renego_status == MBEDTLS_SSL_INITIAL_HANDSHAKE )
S#endif
S    {
S        if( ssl->session_negotiate->ticket != NULL &&
S                ssl->session_negotiate->ticket_len != 0 )
S        {
S            ret = ssl->conf->f_rng( ssl->conf->p_rng, ssl->session_negotiate->id, 32 );
S
S            if( ret != 0 )
S                return( ret );
S
S            ssl->session_negotiate->id_len = n = 32;
S        }
S    }
S#endif /* MBEDTLS_SSL_SESSION_TICKETS */
S
S    *p++ = (unsigned char) n;
S
S    for( i = 0; i < n; i++ )
S        *p++ = ssl->session_negotiate->id[i];
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, session id len.: %d", n ) );
S    MBEDTLS_SSL_DEBUG_BUF( 3,   "client hello, session id", buf + 39, n );
S
S    /*
S     * DTLS cookie
S     */
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
S    if( ssl->conf->transport == MBEDTLS_SSL_TRANSPORT_DATAGRAM )
S    {
S        if( ssl->handshake->verify_cookie == NULL )
S        {
S            MBEDTLS_SSL_DEBUG_MSG( 3, ( "no verify cookie to send" ) );
S            *p++ = 0;
S        }
S        else
S        {
S            MBEDTLS_SSL_DEBUG_BUF( 3, "client hello, cookie",
S                              ssl->handshake->verify_cookie,
S                              ssl->handshake->verify_cookie_len );
S
S            *p++ = ssl->handshake->verify_cookie_len;
S            memcpy( p, ssl->handshake->verify_cookie,
S                       ssl->handshake->verify_cookie_len );
S            p += ssl->handshake->verify_cookie_len;
S        }
S    }
S#endif
S
S    /*
S     * Ciphersuite list
S     */
S    ciphersuites = ssl->conf->ciphersuite_list[ssl->minor_ver];
S
S    /* Skip writing ciphersuite length for now */
S    n = 0;
S    q = p;
S    p += 2;
S
S    for( i = 0; ciphersuites[i] != 0; i++ )
S    {
S        ciphersuite_info = mbedtls_ssl_ciphersuite_from_id( ciphersuites[i] );
S
S        if( ciphersuite_info == NULL )
S            continue;
S
S        if( ciphersuite_info->min_minor_ver > ssl->conf->max_minor_ver ||
S            ciphersuite_info->max_minor_ver < ssl->conf->min_minor_ver )
S            continue;
S
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
S        if( ssl->conf->transport == MBEDTLS_SSL_TRANSPORT_DATAGRAM &&
S            ( ciphersuite_info->flags & MBEDTLS_CIPHERSUITE_NODTLS ) )
S            continue;
S#endif
S
S#if defined(MBEDTLS_ARC4_C)
S        if( ssl->conf->arc4_disabled == MBEDTLS_SSL_ARC4_DISABLED &&
S            ciphersuite_info->cipher == MBEDTLS_CIPHER_ARC4_128 )
S            continue;
S#endif
S
S#if defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
S        if( ciphersuite_info->key_exchange == MBEDTLS_KEY_EXCHANGE_ECJPAKE &&
S            mbedtls_ecjpake_check( &ssl->handshake->ecjpake_ctx ) != 0 )
S            continue;
S#endif
S
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, add ciphersuite: %04x",
S                                    ciphersuites[i] ) );
S
S        n++;
S        *p++ = (unsigned char)( ciphersuites[i] >> 8 );
S        *p++ = (unsigned char)( ciphersuites[i]      );
S    }
S
S    /*
S     * Add TLS_EMPTY_RENEGOTIATION_INFO_SCSV
S     */
S#if defined(MBEDTLS_SSL_RENEGOTIATION)
S    if( ssl->renego_status == MBEDTLS_SSL_INITIAL_HANDSHAKE )
S#endif
S    {
S        *p++ = (unsigned char)( MBEDTLS_SSL_EMPTY_RENEGOTIATION_INFO >> 8 );
S        *p++ = (unsigned char)( MBEDTLS_SSL_EMPTY_RENEGOTIATION_INFO      );
S        n++;
S    }
S
S    /* Some versions of OpenSSL don't handle it correctly if not at end */
S#if defined(MBEDTLS_SSL_FALLBACK_SCSV)
S    if( ssl->conf->fallback == MBEDTLS_SSL_IS_FALLBACK )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "adding FALLBACK_SCSV" ) );
S        *p++ = (unsigned char)( MBEDTLS_SSL_FALLBACK_SCSV_VALUE >> 8 );
S        *p++ = (unsigned char)( MBEDTLS_SSL_FALLBACK_SCSV_VALUE      );
S        n++;
S    }
S#endif
S
S    *q++ = (unsigned char)( n >> 7 );
S    *q++ = (unsigned char)( n << 1 );
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, got %d ciphersuites", n ) );
S
S#if defined(MBEDTLS_ZLIB_SUPPORT)
S    offer_compress = 1;
S#else
S    offer_compress = 0;
S#endif
S
S    /*
S     * We don't support compression with DTLS right now: is many records come
S     * in the same datagram, uncompressing one could overwrite the next one.
S     * We don't want to add complexity for handling that case unless there is
S     * an actual need for it.
S     */
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
S    if( ssl->conf->transport == MBEDTLS_SSL_TRANSPORT_DATAGRAM )
S        offer_compress = 0;
S#endif
S
S    if( offer_compress )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, compress len.: %d", 2 ) );
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, compress alg.: %d %d",
S                            MBEDTLS_SSL_COMPRESS_DEFLATE, MBEDTLS_SSL_COMPRESS_NULL ) );
S
S        *p++ = 2;
S        *p++ = MBEDTLS_SSL_COMPRESS_DEFLATE;
S        *p++ = MBEDTLS_SSL_COMPRESS_NULL;
S    }
S    else
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, compress len.: %d", 1 ) );
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, compress alg.: %d",
S                            MBEDTLS_SSL_COMPRESS_NULL ) );
S
S        *p++ = 1;
S        *p++ = MBEDTLS_SSL_COMPRESS_NULL;
S    }
S
S    // First write extensions, then the total length
S    //
S#if defined(MBEDTLS_SSL_SERVER_NAME_INDICATION)
S    ssl_write_hostname_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_SSL_RENEGOTIATION)
S    ssl_write_renegotiation_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_SSL_PROTO_TLS1_2) && \
S    defined(MBEDTLS_KEY_EXCHANGE__WITH_CERT__ENABLED)
X#if defined(MBEDTLS_SSL_PROTO_TLS1_2) &&     defined(MBEDTLS_KEY_EXCHANGE__WITH_CERT__ENABLED)
S    ssl_write_signature_algorithms_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_ECDH_C) || defined(MBEDTLS_ECDSA_C) || \
S    defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
X#if defined(MBEDTLS_ECDH_C) || defined(MBEDTLS_ECDSA_C) ||     defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
S    ssl_write_supported_elliptic_curves_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S
S    ssl_write_supported_point_formats_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
S    ssl_write_ecjpake_kkpp_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_SSL_MAX_FRAGMENT_LENGTH)
S    ssl_write_max_fragment_length_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_SSL_TRUNCATED_HMAC)
S    ssl_write_truncated_hmac_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_SSL_ENCRYPT_THEN_MAC)
S    ssl_write_encrypt_then_mac_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_SSL_EXTENDED_MASTER_SECRET)
S    ssl_write_extended_ms_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_SSL_ALPN)
S    ssl_write_alpn_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S#if defined(MBEDTLS_SSL_SESSION_TICKETS)
S    ssl_write_session_ticket_ext( ssl, p + 2 + ext_len, &olen );
S    ext_len += olen;
S#endif
S
S    /* olen unused if all extensions are disabled */
S    ((void) olen);
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, total extension length: %d",
S                   ext_len ) );
S
S    if( ext_len > 0 )
S    {
S        *p++ = (unsigned char)( ( ext_len >> 8 ) & 0xFF );
S        *p++ = (unsigned char)( ( ext_len      ) & 0xFF );
S        p += ext_len;
S    }
S
S    ssl->out_msglen  = p - buf;
S    ssl->out_msgtype = MBEDTLS_SSL_MSG_HANDSHAKE;
S    ssl->out_msg[0]  = MBEDTLS_SSL_HS_CLIENT_HELLO;
S
S    ssl->state++;
S
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
S    if( ssl->conf->transport == MBEDTLS_SSL_TRANSPORT_DATAGRAM )
S        mbedtls_ssl_send_flight_completed( ssl );
S#endif
S
S    if( ( ret = mbedtls_ssl_write_record( ssl ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "mbedtls_ssl_write_record", ret );
S        return( ret );
S    }
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "<= write client hello" ) );
S
S    return( 0 );
S}
S
Sstatic int ssl_parse_renegotiation_info( mbedtls_ssl_context *ssl,
S                                         const unsigned char *buf,
S                                         size_t len )
S{
S    int ret;
S
S#if defined(MBEDTLS_SSL_RENEGOTIATION)
S    if( ssl->renego_status != MBEDTLS_SSL_INITIAL_HANDSHAKE )
S    {
S        /* Check verify-data in constant-time. The length OTOH is no secret */
S        if( len    != 1 + ssl->verify_data_len * 2 ||
S            buf[0] !=     ssl->verify_data_len * 2 ||
S            mbedtls_ssl_safer_memcmp( buf + 1,
S                          ssl->own_verify_data, ssl->verify_data_len ) != 0 ||
S            mbedtls_ssl_safer_memcmp( buf + 1 + ssl->verify_data_len,
S                          ssl->peer_verify_data, ssl->verify_data_len ) != 0 )
S        {
S            MBEDTLS_SSL_DEBUG_MSG( 1, ( "non-matching renegotiation info" ) );
S
S            if( ( ret = mbedtls_ssl_send_fatal_handshake_failure( ssl ) ) != 0 )
S                return( ret );
S
S            return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S        }
S    }
S    else
S#endif /* MBEDTLS_SSL_RENEGOTIATION */
S    {
S        if( len != 1 || buf[0] != 0x00 )
S        {
S            MBEDTLS_SSL_DEBUG_MSG( 1, ( "non-zero length renegotiation info" ) );
S
S            if( ( ret = mbedtls_ssl_send_fatal_handshake_failure( ssl ) ) != 0 )
S                return( ret );
S
S            return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S        }
S
S        ssl->secure_renegotiation = MBEDTLS_SSL_SECURE_RENEGOTIATION;
S    }
S
S    return( 0 );
S}
S
S#if defined(MBEDTLS_SSL_MAX_FRAGMENT_LENGTH)
Sstatic int ssl_parse_max_fragment_length_ext( mbedtls_ssl_context *ssl,
S                                              const unsigned char *buf,
S                                              size_t len )
S{
S    /*
S     * server should use the extension only if we did,
S     * and if so the server's value should match ours (and len is always 1)
S     */
S    if( ssl->conf->mfl_code == MBEDTLS_SSL_MAX_FRAG_LEN_NONE ||
S        len != 1 ||
S        buf[0] != ssl->conf->mfl_code )
S    {
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S    }
S
S    return( 0 );
S}
S#endif /* MBEDTLS_SSL_MAX_FRAGMENT_LENGTH */
S
S#if defined(MBEDTLS_SSL_TRUNCATED_HMAC)
Sstatic int ssl_parse_truncated_hmac_ext( mbedtls_ssl_context *ssl,
S                                         const unsigned char *buf,
S                                         size_t len )
S{
S    if( ssl->conf->trunc_hmac == MBEDTLS_SSL_TRUNC_HMAC_DISABLED ||
S        len != 0 )
S    {
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S    }
S
S    ((void) buf);
S
S    ssl->session_negotiate->trunc_hmac = MBEDTLS_SSL_TRUNC_HMAC_ENABLED;
S
S    return( 0 );
S}
S#endif /* MBEDTLS_SSL_TRUNCATED_HMAC */
S
S#if defined(MBEDTLS_SSL_ENCRYPT_THEN_MAC)
Sstatic int ssl_parse_encrypt_then_mac_ext( mbedtls_ssl_context *ssl,
S                                         const unsigned char *buf,
S                                         size_t len )
S{
S    if( ssl->conf->encrypt_then_mac == MBEDTLS_SSL_ETM_DISABLED ||
S        ssl->minor_ver == MBEDTLS_SSL_MINOR_VERSION_0 ||
S        len != 0 )
S    {
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S    }
S
S    ((void) buf);
S
S    ssl->session_negotiate->encrypt_then_mac = MBEDTLS_SSL_ETM_ENABLED;
S
S    return( 0 );
S}
S#endif /* MBEDTLS_SSL_ENCRYPT_THEN_MAC */
S
S#if defined(MBEDTLS_SSL_EXTENDED_MASTER_SECRET)
Sstatic int ssl_parse_extended_ms_ext( mbedtls_ssl_context *ssl,
S                                         const unsigned char *buf,
S                                         size_t len )
S{
S    if( ssl->conf->extended_ms == MBEDTLS_SSL_EXTENDED_MS_DISABLED ||
S        ssl->minor_ver == MBEDTLS_SSL_MINOR_VERSION_0 ||
S        len != 0 )
S    {
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S    }
S
S    ((void) buf);
S
S    ssl->handshake->extended_ms = MBEDTLS_SSL_EXTENDED_MS_ENABLED;
S
S    return( 0 );
S}
S#endif /* MBEDTLS_SSL_EXTENDED_MASTER_SECRET */
S
S#if defined(MBEDTLS_SSL_SESSION_TICKETS)
Sstatic int ssl_parse_session_ticket_ext( mbedtls_ssl_context *ssl,
S                                         const unsigned char *buf,
S                                         size_t len )
S{
S    if( ssl->conf->session_tickets == MBEDTLS_SSL_SESSION_TICKETS_DISABLED ||
S        len != 0 )
S    {
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S    }
S
S    ((void) buf);
S
S    ssl->handshake->new_session_ticket = 1;
S
S    return( 0 );
S}
S#endif /* MBEDTLS_SSL_SESSION_TICKETS */
S
S#if defined(MBEDTLS_ECDH_C) || defined(MBEDTLS_ECDSA_C) || \
S    defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
X#if defined(MBEDTLS_ECDH_C) || defined(MBEDTLS_ECDSA_C) ||     defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
Sstatic int ssl_parse_supported_point_formats_ext( mbedtls_ssl_context *ssl,
S                                                  const unsigned char *buf,
S                                                  size_t len )
S{
S    size_t list_size;
S    const unsigned char *p;
S
S    list_size = buf[0];
S    if( list_size + 1 != len )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad server hello message" ) );
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S    }
S
S    p = buf + 1;
S    while( list_size > 0 )
S    {
S        if( p[0] == MBEDTLS_ECP_PF_UNCOMPRESSED ||
S            p[0] == MBEDTLS_ECP_PF_COMPRESSED )
S        {
S#if defined(MBEDTLS_ECDH_C) || defined(MBEDTLS_ECDSA_C)
S            ssl->handshake->ecdh_ctx.point_format = p[0];
S#endif            
S#if defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
S            ssl->handshake->ecjpake_ctx.point_format = p[0];
S#endif
S            MBEDTLS_SSL_DEBUG_MSG( 4, ( "point format selected: %d", p[0] ) );
S            return( 0 );
S        }
S
S        list_size--;
S        p++;
S    }
S
S    MBEDTLS_SSL_DEBUG_MSG( 1, ( "no point format in common" ) );
S    return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S}
S#endif /* MBEDTLS_ECDH_C || MBEDTLS_ECDSA_C || 
S          MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED */
S
S#if defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
Sstatic int ssl_parse_ecjpake_kkpp( mbedtls_ssl_context *ssl,
S                                   const unsigned char *buf,
S                                   size_t len )
S{
S    int ret;
S
S    if( ssl->transform_negotiate->ciphersuite_info->key_exchange !=
S        MBEDTLS_KEY_EXCHANGE_ECJPAKE )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "skip ecjpake kkpp extension" ) );
S        return( 0 );
S    }
S
S    /* If we got here, we no longer need our cached extension */
S    mbedtls_free( ssl->handshake->ecjpake_cache );
S    ssl->handshake->ecjpake_cache = NULL;
S    ssl->handshake->ecjpake_cache_len = 0;
S
S    if( ( ret = mbedtls_ecjpake_read_round_one( &ssl->handshake->ecjpake_ctx,
S                                                buf, len ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "mbedtls_ecjpake_read_round_one", ret );
S        return( ret );
S    }
S
S    return( 0 );
S}
S#endif /* MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED */
S
S#if defined(MBEDTLS_SSL_ALPN)
Sstatic int ssl_parse_alpn_ext( mbedtls_ssl_context *ssl,
S                               const unsigned char *buf, size_t len )
S{
S    size_t list_len, name_len;
S    const char **p;
S
S    /* If we didn't send it, the server shouldn't send it */
S    if( ssl->conf->alpn_list == NULL )
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S
S    /*
S     * opaque ProtocolName<1..2^8-1>;
S     *
S     * struct {
S     *     ProtocolName protocol_name_list<2..2^16-1>
S     * } ProtocolNameList;
S     *
S     * the "ProtocolNameList" MUST contain exactly one "ProtocolName"
S     */
S
S    /* Min length is 2 (list_len) + 1 (name_len) + 1 (name) */
S    if( len < 4 )
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S
S    list_len = ( buf[0] << 8 ) | buf[1];
S    if( list_len != len - 2 )
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S
S    name_len = buf[2];
S    if( name_len != list_len - 1 )
S        return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S
S    /* Check that the server chosen protocol was in our list and save it */
S    for( p = ssl->conf->alpn_list; *p != NULL; p++ )
S    {
S        if( name_len == strlen( *p ) &&
S            memcmp( buf + 3, *p, name_len ) == 0 )
S        {
S            ssl->alpn_chosen = *p;
S            return( 0 );
S        }
S    }
S
S    return( MBEDTLS_ERR_SSL_BAD_HS_SERVER_HELLO );
S}
S#endif /* MBEDTLS_SSL_ALPN */
S
S/*
S * Parse HelloVerifyRequest.  Only called after verifying the HS type.
S */
S#if defined(MBEDTLS_SSL_PROTO_DTLS)
Sstatic int ssl_parse_hello_verify_request( mbedtls_ssl_context *ssl )
S{
S    const unsigned char *p = ssl->in_msg + mbedtls_ssl_hs_hdr_len( ssl );
S    int major_ver, minor_ver;
S    unsigned char cookie_len;
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "=> parse hello verify request" ) );
S
S    /*
S     * struct {
S     *   ProtocolVersion server_version;
S     *   opaque cookie<0..2^8-1>;
S     * } HelloVerifyRequest;
S     */
S    MBEDTLS_SSL_DEBUG_BUF( 3, "server version", p, 2 );
S    mbedtls_ssl_read_version( &major_ver, &minor_ver, ssl->conf->transport, p );
S    p += 2;
S
S    /*
S     * Since the RFC is not clear on this point, accept DTLS 1.0 (TLS 1.1)
S     * even is lower than our min version.
S     */
S    if( major_ver < MBEDTLS_SSL_MAJOR_VERSION_3 ||
S        minor_ver < MBEDTLS_SSL_MINOR_VERSION_2 ||
S        major_ver > ssl->conf->max_major_ver  ||
S        minor_ver > ssl->conf->max_minor_ver  )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "bad server version" ) );
S
S        mbedtls_ssl_send_alert_message( ssl, MBEDTLS_SSL_ALERT_LEVEL_FATAL,
S                                     MBEDTLS_SSL_ALERT_MSG_PROTOCOL_VERSION );
S
S        return( MBEDTLS_ERR_SSL_BAD_HS_PROTOCOL_VERSION );
S    }
S
S    cookie_len = *p++;
S    MBEDTLS_SSL_DEBUG_BUF( 3, "cookie", p, cookie_len );
S
S    mbedtls_free( ssl->handshake->verify_cookie );
S
S    ssl->handshake->verify_cookie = mbedtls_calloc( 1, cookie_len );
S    if( ssl->handshake->verify_cookie  == NULL )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "alloc failed (%d bytes)", cookie_len ) );
S        return( MBEDTLS_ERR_SSL_ALLOC_FAILED );
S    }
S
S    memcpy( ssl->handshake->verify_cookie, p, cookie_len );
S    ssl->handshake->verify_cookie_len = cookie_len;
S
S    /* Start over at ClientHello */
S    ssl->state = MBEDTLS_SSL_CLIENT_HELLO;
S    mbedtls_ssl_reset_checksum( ssl );
S
S    mbedtls_ssl_recv_flight_completed( ssl );
S
S    MBEDTLS_SSL_DEBUG_MSG( 2, ( "<= parse hello verify request" ) );
S
S    return( 0 );
S}
S#endif /* MBEDTLS_SSL_PROTO_DTLS */
