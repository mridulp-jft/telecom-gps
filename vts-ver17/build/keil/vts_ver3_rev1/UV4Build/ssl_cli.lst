S#if defined(MBEDTLS_KEY_EXCHANGE_ECJPAKE_ENABLED)
Sstatic void ssl_write_ecjpake_kkpp_ext( mbedtls_ssl_context *ssl,
S                                        unsigned char *buf,
S                                        size_t *olen )
S{
S    int ret;
S    unsigned char *p = buf;
S    const unsigned char *end = ssl->out_msg + MBEDTLS_SSL_MAX_CONTENT_LEN;
S    size_t kkpp_len;
S
S    *olen = 0;
S
S    /* Skip costly extension if we can't use EC J-PAKE anyway */
S    if( mbedtls_ecjpake_check( &ssl->handshake->ecjpake_ctx ) != 0 )
S        return;
S
S    MBEDTLS_SSL_DEBUG_MSG( 3, ( "client hello, adding ecjpake_kkpp extension" ) );
S
S    if( end - p < 4 )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "buffer too small" ) );
S        return;
S    }
S
S    *p++ = (unsigned char)( ( MBEDTLS_TLS_EXT_ECJPAKE_KKPP >> 8 ) & 0xFF );
S    *p++ = (unsigned char)( ( MBEDTLS_TLS_EXT_ECJPAKE_KKPP      ) & 0xFF );
S
S    /*
S     * We may need to send ClientHello multiple times for Hello verification.
S     * We don't want to compute fresh values every time (both for performance
S     * and consistency reasons), so cache the extension content.
S     */
S    if( ssl->handshake->ecjpake_cache == NULL ||
S        ssl->handshake->ecjpake_cache_len == 0 )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "generating new ecjpake parameters" ) );
S
S        ret = mbedtls_ecjpake_write_round_one( &ssl->handshake->ecjpake_ctx,
S                                        p + 2, end - p - 2, &kkpp_len,
S                                        ssl->conf->f_rng, ssl->conf->p_rng );
S        if( ret != 0 )
S        {
S            MBEDTLS_SSL_DEBUG_RET( 1 , "mbedtls_ecjpake_write_round_one", ret );
S            return;
S        }
S
S        ssl->handshake->ecjpake_cache = mbedtls_calloc( 1, kkpp_len );
S        if( ssl->handshake->ecjpake_cache == NULL )
S        {
S            MBEDTLS_SSL_DEBUG_MSG( 1, ( "allocation failed" ) );
S            return;
S        }
S
S        memcpy( ssl->handshake->ecjpake_cache, p + 2, kkpp_len );
S        ssl->handshake->ecjpake_cache_len = kkpp_len;
S    }
S    else
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 3, ( "re-using cached ecjpake parameters" ) );
S
S        kkpp_len = ssl->handshake->ecjpake_cache_len;
S
S        if( (size_t)( end - p - 2 ) < kkpp_len )
S        {
S            MBEDTLS_SSL_DEBUG_MSG( 1, ( "buffer too small" ) );
S            return;
S        }
S
S        memcpy( p + 2, ssl->handshake->ecjpake_cache, kkpp_len );
S    }
S
S    *p++ = (unsigned char)( ( kkpp_len >> 8 ) & 0xFF );
S    *p++ = (unsigned char)( ( kkpp_len      ) & 0xFF );
S
S    *olen = kkpp_len + 4;
S}
