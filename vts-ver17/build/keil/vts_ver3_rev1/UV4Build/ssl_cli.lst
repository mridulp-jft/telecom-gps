S     * Now write it out, encrypted
S     */
S    if( ! mbedtls_pk_can_do( &ssl->session_negotiate->peer_cert->pk,
S                MBEDTLS_PK_RSA ) )
S    {
S        MBEDTLS_SSL_DEBUG_MSG( 1, ( "certificate key type mismatch" ) );
S        return( MBEDTLS_ERR_SSL_PK_TYPE_MISMATCH );
S    }
S
S    if( ( ret = mbedtls_pk_encrypt( &ssl->session_negotiate->peer_cert->pk,
S                            p, ssl->handshake->pmslen,
S                            ssl->out_msg + offset + len_bytes, olen,
S                            MBEDTLS_SSL_MAX_CONTENT_LEN - offset - len_bytes,
S                            ssl->conf->f_rng, ssl->conf->p_rng ) ) != 0 )
S    {
S        MBEDTLS_SSL_DEBUG_RET( 1, "mbedtls_rsa_pkcs1_encrypt", ret );
S        return( ret );
S    }
S
S#if defined(MBEDTLS_SSL_PROTO_TLS1) || defined(MBEDTLS_SSL_PROTO_TLS1_1) || \
S    defined(MBEDTLS_SSL_PROTO_TLS1_2)
X#if defined(MBEDTLS_SSL_PROTO_TLS1) || defined(MBEDTLS_SSL_PROTO_TLS1_1) ||     defined(MBEDTLS_SSL_PROTO_TLS1_2)
S    if( len_bytes == 2 )
