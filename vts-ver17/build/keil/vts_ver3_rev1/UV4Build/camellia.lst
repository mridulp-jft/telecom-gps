S#define SHIFT_AND_PLACE(INDEX, OFFSET)                      \
S{                                                           \
S    TK[0] = KC[(OFFSET) * 4 + 0];                           \
S    TK[1] = KC[(OFFSET) * 4 + 1];                           \
S    TK[2] = KC[(OFFSET) * 4 + 2];                           \
S    TK[3] = KC[(OFFSET) * 4 + 3];                           \
S                                                            \
S    for( i = 1; i <= 4; i++ )                               \
S        if( shifts[(INDEX)][(OFFSET)][i -1] )               \
S            ROTL(TK + i * 4, TK, ( 15 * i ) % 32);          \
S                                                            \
S    for( i = 0; i < 20; i++ )                               \
S        if( indexes[(INDEX)][(OFFSET)][i] != -1 ) {         \
S            RK[indexes[(INDEX)][(OFFSET)][i]] = TK[ i ];    \
S        }                                                   \
S}
X#define SHIFT_AND_PLACE(INDEX, OFFSET)                      {                                                               TK[0] = KC[(OFFSET) * 4 + 0];                               TK[1] = KC[(OFFSET) * 4 + 1];                               TK[2] = KC[(OFFSET) * 4 + 2];                               TK[3] = KC[(OFFSET) * 4 + 3];                                                                                           for( i = 1; i <= 4; i++ )                                       if( shifts[(INDEX)][(OFFSET)][i -1] )                           ROTL(TK + i * 4, TK, ( 15 * i ) % 32);                                                                          for( i = 0; i < 20; i++ )                                       if( indexes[(INDEX)][(OFFSET)][i] != -1 ) {                     RK[indexes[(INDEX)][(OFFSET)][i]] = TK[ i ];            }                                                   }
