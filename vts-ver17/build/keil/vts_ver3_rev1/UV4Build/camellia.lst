S#if defined(MBEDTLS_CIPHER_MODE_CBC)
S/*
S * Camellia-CBC buffer encryption/decryption
S */
Sint mbedtls_camellia_crypt_cbc( mbedtls_camellia_context *ctx,
S                    int mode,
S                    size_t length,
S                    unsigned char iv[16],
S                    const unsigned char *input,
S                    unsigned char *output )
S{
S    int i;
S    unsigned char temp[16];
S
S    if( length % 16 )
S        return( MBEDTLS_ERR_CAMELLIA_INVALID_INPUT_LENGTH );
S
S    if( mode == MBEDTLS_CAMELLIA_DECRYPT )
S    {
S        while( length > 0 )
S        {
S            memcpy( temp, input, 16 );
S            mbedtls_camellia_crypt_ecb( ctx, mode, input, output );
S
S            for( i = 0; i < 16; i++ )
S                output[i] = (unsigned char)( output[i] ^ iv[i] );
S
S            memcpy( iv, temp, 16 );
S
S            input  += 16;
S            output += 16;
S            length -= 16;
S        }
S    }
S    else
S    {
S        while( length > 0 )
S        {
S            for( i = 0; i < 16; i++ )
S                output[i] = (unsigned char)( input[i] ^ iv[i] );
S
S            mbedtls_camellia_crypt_ecb( ctx, mode, output, output );
S            memcpy( iv, output, 16 );
S
S            input  += 16;
S            output += 16;
S            length -= 16;
S        }
S    }
S
S    return( 0 );
S}
S#endif /* MBEDTLS_CIPHER_MODE_CBC */
S
S#if defined(MBEDTLS_CIPHER_MODE_CFB)
S/*
S * Camellia-CFB128 buffer encryption/decryption
S */
Sint mbedtls_camellia_crypt_cfb128( mbedtls_camellia_context *ctx,
S                       int mode,
S                       size_t length,
S                       size_t *iv_off,
S                       unsigned char iv[16],
S                       const unsigned char *input,
S                       unsigned char *output )
S{
S    int c;
S    size_t n = *iv_off;
S
S    if( mode == MBEDTLS_CAMELLIA_DECRYPT )
S    {
S        while( length-- )
S        {
S            if( n == 0 )
S                mbedtls_camellia_crypt_ecb( ctx, MBEDTLS_CAMELLIA_ENCRYPT, iv, iv );
S
S            c = *input++;
S            *output++ = (unsigned char)( c ^ iv[n] );
S            iv[n] = (unsigned char) c;
S
S            n = ( n + 1 ) & 0x0F;
S        }
S    }
S    else
S    {
S        while( length-- )
S        {
S            if( n == 0 )
S                mbedtls_camellia_crypt_ecb( ctx, MBEDTLS_CAMELLIA_ENCRYPT, iv, iv );
S
S            iv[n] = *output++ = (unsigned char)( iv[n] ^ *input++ );
S
S            n = ( n + 1 ) & 0x0F;
S        }
S    }
S
S    *iv_off = n;
S
S    return( 0 );
S}
S#endif /* MBEDTLS_CIPHER_MODE_CFB */
S
S#if defined(MBEDTLS_CIPHER_MODE_CTR)
S/*
S * Camellia-CTR buffer encryption/decryption
S */
Sint mbedtls_camellia_crypt_ctr( mbedtls_camellia_context *ctx,
S                       size_t length,
S                       size_t *nc_off,
S                       unsigned char nonce_counter[16],
S                       unsigned char stream_block[16],
S                       const unsigned char *input,
S                       unsigned char *output )
S{
S    int c, i;
S    size_t n = *nc_off;
S
S    while( length-- )
S    {
S        if( n == 0 ) {
S            mbedtls_camellia_crypt_ecb( ctx, MBEDTLS_CAMELLIA_ENCRYPT, nonce_counter,
S                                stream_block );
S
S            for( i = 16; i > 0; i-- )
S                if( ++nonce_counter[i - 1] != 0 )
S                    break;
S        }
S        c = *input++;
S        *output++ = (unsigned char)( c ^ stream_block[n] );
S
S        n = ( n + 1 ) & 0x0F;
S    }
S
S    *nc_off = n;
S
S    return( 0 );
S}
S#endif /* MBEDTLS_CIPHER_MODE_CTR */
S#endif /* !MBEDTLS_CAMELLIA_ALT */
