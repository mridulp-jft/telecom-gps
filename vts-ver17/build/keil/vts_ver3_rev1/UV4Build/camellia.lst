S/*
S * Checkup routine
S */
Sint mbedtls_camellia_self_test( int verbose )
S{
S    int i, j, u, v;
S    unsigned char key[32];
S    unsigned char buf[64];
S    unsigned char src[16];
S    unsigned char dst[16];
S#if defined(MBEDTLS_CIPHER_MODE_CBC)
S    unsigned char iv[16];
S#endif
S#if defined(MBEDTLS_CIPHER_MODE_CTR)
S    size_t offset, len;
S    unsigned char nonce_counter[16];
S    unsigned char stream_block[16];
S#endif
S
S    mbedtls_camellia_context ctx;
S
S    memset( key, 0, 32 );
S
S    for( j = 0; j < 6; j++ ) {
S        u = j >> 1;
S    v = j & 1;
S
S    if( verbose != 0 )
S        mbedtls_printf( "  CAMELLIA-ECB-%3d (%s): ", 128 + u * 64,
S                         (v == MBEDTLS_CAMELLIA_DECRYPT) ? "dec" : "enc");
S
S    for( i = 0; i < CAMELLIA_TESTS_ECB; i++ ) {
S        memcpy( key, camellia_test_ecb_key[u][i], 16 + 8 * u );
S
S        if( v == MBEDTLS_CAMELLIA_DECRYPT ) {
S            mbedtls_camellia_setkey_dec( &ctx, key, 128 + u * 64 );
S            memcpy( src, camellia_test_ecb_cipher[u][i], 16 );
S            memcpy( dst, camellia_test_ecb_plain[i], 16 );
S        } else { /* MBEDTLS_CAMELLIA_ENCRYPT */
S            mbedtls_camellia_setkey_enc( &ctx, key, 128 + u * 64 );
S            memcpy( src, camellia_test_ecb_plain[i], 16 );
S            memcpy( dst, camellia_test_ecb_cipher[u][i], 16 );
S        }
S
S        mbedtls_camellia_crypt_ecb( &ctx, v, src, buf );
S
S        if( memcmp( buf, dst, 16 ) != 0 )
S        {
S            if( verbose != 0 )
S                mbedtls_printf( "failed\n" );
S
S            return( 1 );
S        }
S    }
S
S    if( verbose != 0 )
S        mbedtls_printf( "passed\n" );
S    }
S
S    if( verbose != 0 )
S        mbedtls_printf( "\n" );
S
S#if defined(MBEDTLS_CIPHER_MODE_CBC)
S    /*
S     * CBC mode
S     */
S    for( j = 0; j < 6; j++ )
S    {
S        u = j >> 1;
S        v = j  & 1;
S
S        if( verbose != 0 )
S            mbedtls_printf( "  CAMELLIA-CBC-%3d (%s): ", 128 + u * 64,
S                             ( v == MBEDTLS_CAMELLIA_DECRYPT ) ? "dec" : "enc" );
S
S    memcpy( src, camellia_test_cbc_iv, 16 );
S    memcpy( dst, camellia_test_cbc_iv, 16 );
S    memcpy( key, camellia_test_cbc_key[u], 16 + 8 * u );
S
S    if( v == MBEDTLS_CAMELLIA_DECRYPT ) {
S        mbedtls_camellia_setkey_dec( &ctx, key, 128 + u * 64 );
S    } else {
S        mbedtls_camellia_setkey_enc( &ctx, key, 128 + u * 64 );
S    }
S
S    for( i = 0; i < CAMELLIA_TESTS_CBC; i++ ) {
S
S        if( v == MBEDTLS_CAMELLIA_DECRYPT ) {
S            memcpy( iv , src, 16 );
S            memcpy( src, camellia_test_cbc_cipher[u][i], 16 );
S            memcpy( dst, camellia_test_cbc_plain[i], 16 );
S        } else { /* MBEDTLS_CAMELLIA_ENCRYPT */
S            memcpy( iv , dst, 16 );
