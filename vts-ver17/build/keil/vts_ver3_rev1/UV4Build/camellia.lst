S/*
S * Camellia key schedule (encryption)
S */
Sint mbedtls_camellia_setkey_enc( mbedtls_camellia_context *ctx, const unsigned char *key,
S                         unsigned int keybits )
S{
S    int idx;
S    size_t i;
S    uint32_t *RK;
S    unsigned char t[64];
S    uint32_t SIGMA[6][2];
S    uint32_t KC[16];
S    uint32_t TK[20];
S
S    RK = ctx->rk;
S
S    memset( t, 0, 64 );
S    memset( RK, 0, sizeof(ctx->rk) );
S
S    switch( keybits )
S    {
S        case 128: ctx->nr = 3; idx = 0; break;
S        case 192:
S        case 256: ctx->nr = 4; idx = 1; break;
S        default : return( MBEDTLS_ERR_CAMELLIA_INVALID_KEY_LENGTH );
S    }
S
S    for( i = 0; i < keybits / 8; ++i )
S        t[i] = key[i];
S
S    if( keybits == 192 ) {
S        for( i = 0; i < 8; i++ )
S            t[24 + i] = ~t[16 + i];
S    }
S
S    /*
S     * Prepare SIGMA values
S     */
S    for( i = 0; i < 6; i++ ) {
S        GET_UINT32_BE( SIGMA[i][0], SIGMA_CHARS[i], 0 );
S        GET_UINT32_BE( SIGMA[i][1], SIGMA_CHARS[i], 4 );
S    }
S
S    /*
S     * Key storage in KC
S     * Order: KL, KR, KA, KB
S     */
S    memset( KC, 0, sizeof(KC) );
S
S    /* Store KL, KR */
S    for( i = 0; i < 8; i++ )
S        GET_UINT32_BE( KC[i], t, i * 4 );
S
S    /* Generate KA */
S    for( i = 0; i < 4; ++i )
S        KC[8 + i] = KC[i] ^ KC[4 + i];
S
S    camellia_feistel( KC + 8, SIGMA[0], KC + 10 );
S    camellia_feistel( KC + 10, SIGMA[1], KC + 8 );
S
S    for( i = 0; i < 4; ++i )
S        KC[8 + i] ^= KC[i];
S
S    camellia_feistel( KC + 8, SIGMA[2], KC + 10 );
S    camellia_feistel( KC + 10, SIGMA[3], KC + 8 );
S
S    if( keybits > 128 ) {
S        /* Generate KB */
S        for( i = 0; i < 4; ++i )
S            KC[12 + i] = KC[4 + i] ^ KC[8 + i];
S
S        camellia_feistel( KC + 12, SIGMA[4], KC + 14 );
S        camellia_feistel( KC + 14, SIGMA[5], KC + 12 );
S    }
S
S    /*
S     * Generating subkeys
S     */
S
S    /* Manipulating KL */
S    SHIFT_AND_PLACE( idx, 0 );
S
S    /* Manipulating KR */
S    if( keybits > 128 ) {
S        SHIFT_AND_PLACE( idx, 1 );
S    }
S
S    /* Manipulating KA */
S    SHIFT_AND_PLACE( idx, 2 );
S
S    /* Manipulating KB */
S    if( keybits > 128 ) {
S        SHIFT_AND_PLACE( idx, 3 );
S    }
S
S    /* Do transpositions */
S    for( i = 0; i < 20; i++ ) {
S        if( transposes[idx][i] != -1 ) {
S            RK[32 + 12 * idx + i] = RK[transposes[idx][i]];
S        }
S    }
S
S    return( 0 );
S}
S
S/*
S * Camellia key schedule (decryption)
S */
Sint mbedtls_camellia_setkey_dec( mbedtls_camellia_context *ctx, const unsigned char *key,
S                         unsigned int keybits )
S{
S    int idx, ret;
S    size_t i;
S    mbedtls_camellia_context cty;
S    uint32_t *RK;
S    uint32_t *SK;
S
S    mbedtls_camellia_init( &cty );
S
S    /* Also checks keybits */
S    if( ( ret = mbedtls_camellia_setkey_enc( &cty, key, keybits ) ) != 0 )
S        goto exit;
S
S    ctx->nr = cty.nr;
S    idx = ( ctx->nr == 4 );
S
S    RK = ctx->rk;
S    SK = cty.rk + 24 * 2 + 8 * idx * 2;
S
S    *RK++ = *SK++;
S    *RK++ = *SK++;
S    *RK++ = *SK++;
S    *RK++ = *SK++;
S
S    for( i = 22 + 8 * idx, SK -= 6; i > 0; i--, SK -= 4 )
S    {
S        *RK++ = *SK++;
S        *RK++ = *SK++;
S    }
S
S    SK -= 2;
S
S    *RK++ = *SK++;
S    *RK++ = *SK++;
S    *RK++ = *SK++;
S    *RK++ = *SK++;
S
Sexit:
S    mbedtls_camellia_free( &cty );
S
S    return( ret );
S}
