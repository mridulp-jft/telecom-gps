S#define FL(XL, XR, KL, KR)                                          \
S{                                                                   \
S    (XR) = ((((XL) & (KL)) << 1) | (((XL) & (KL)) >> 31)) ^ (XR);   \
S    (XL) = ((XR) | (KR)) ^ (XL);                                    \
S}
X#define FL(XL, XR, KL, KR)                                          {                                                                       (XR) = ((((XL) & (KL)) << 1) | (((XL) & (KL)) >> 31)) ^ (XR);       (XL) = ((XR) | (KR)) ^ (XL);                                    }
S
S#define FLInv(YL, YR, KL, KR)                                       \
S{                                                                   \
S    (YL) = ((YR) | (KR)) ^ (YL);                                    \
S    (YR) = ((((YL) & (KL)) << 1) | (((YL) & (KL)) >> 31)) ^ (YR);   \
S}
X#define FLInv(YL, YR, KL, KR)                                       {                                                                       (YL) = ((YR) | (KR)) ^ (YL);                                        (YR) = ((((YL) & (KL)) << 1) | (((YL) & (KL)) >> 31)) ^ (YR);   }
