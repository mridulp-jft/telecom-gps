S/*
S * Camellia-ECB block encryption/decryption
S */
Sint mbedtls_camellia_crypt_ecb( mbedtls_camellia_context *ctx,
S                    int mode,
S                    const unsigned char input[16],
S                    unsigned char output[16] )
S{
S    int NR;
S    uint32_t *RK, X[4];
S
S    ( (void) mode );
S
S    NR = ctx->nr;
S    RK = ctx->rk;
S
S    GET_UINT32_BE( X[0], input,  0 );
S    GET_UINT32_BE( X[1], input,  4 );
S    GET_UINT32_BE( X[2], input,  8 );
S    GET_UINT32_BE( X[3], input, 12 );
S
S    X[0] ^= *RK++;
S    X[1] ^= *RK++;
S    X[2] ^= *RK++;
S    X[3] ^= *RK++;
S
S    while( NR ) {
S        --NR;
S        camellia_feistel( X, RK, X + 2 );
S        RK += 2;
S        camellia_feistel( X + 2, RK, X );
S        RK += 2;
S        camellia_feistel( X, RK, X + 2 );
S        RK += 2;
S        camellia_feistel( X + 2, RK, X );
S        RK += 2;
S        camellia_feistel( X, RK, X + 2 );
S        RK += 2;
S        camellia_feistel( X + 2, RK, X );
S        RK += 2;
S
S        if( NR ) {
S            FL(X[0], X[1], RK[0], RK[1]);
S            RK += 2;
S            FLInv(X[2], X[3], RK[0], RK[1]);
S            RK += 2;
S        }
S    }
S
S    X[2] ^= *RK++;
S    X[3] ^= *RK++;
S    X[0] ^= *RK++;
S    X[1] ^= *RK++;
S
S    PUT_UINT32_BE( X[2], output,  0 );
S    PUT_UINT32_BE( X[3], output,  4 );
S    PUT_UINT32_BE( X[0], output,  8 );
S    PUT_UINT32_BE( X[1], output, 12 );
S
S    return( 0 );
S}
