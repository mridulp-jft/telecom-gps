Sstatic void camellia_feistel( const uint32_t x[2], const uint32_t k[2],
S                              uint32_t z[2])
S{
S    uint32_t I0, I1;
S    I0 = x[0] ^ k[0];
S    I1 = x[1] ^ k[1];
S
S    I0 = ((uint32_t) SBOX1((I0 >> 24) & 0xFF) << 24) |
S         ((uint32_t) SBOX2((I0 >> 16) & 0xFF) << 16) |
S         ((uint32_t) SBOX3((I0 >>  8) & 0xFF) <<  8) |
S         ((uint32_t) SBOX4((I0      ) & 0xFF)      );
S    I1 = ((uint32_t) SBOX2((I1 >> 24) & 0xFF) << 24) |
S         ((uint32_t) SBOX3((I1 >> 16) & 0xFF) << 16) |
S         ((uint32_t) SBOX4((I1 >>  8) & 0xFF) <<  8) |
S         ((uint32_t) SBOX1((I1      ) & 0xFF)      );
S
S    I0 ^= (I1 << 8) | (I1 >> 24);
S    I1 ^= (I0 << 16) | (I0 >> 16);
S    I0 ^= (I1 >> 8) | (I1 << 24);
S    I1 ^= (I0 >> 8) | (I0 << 24);
S
S    z[0] ^= I1;
S    z[1] ^= I0;
S}
