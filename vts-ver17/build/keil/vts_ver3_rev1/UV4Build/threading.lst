Sstatic void threading_mutex_init_pthread( mbedtls_threading_mutex_t *mutex )
S{
S    if( mutex == NULL )
S        return;
S
S    mutex->is_valid = pthread_mutex_init( &mutex->mutex, NULL ) == 0;
S}
S
Sstatic void threading_mutex_free_pthread( mbedtls_threading_mutex_t *mutex )
S{
S    if( mutex == NULL )
S        return;
S
S    (void) pthread_mutex_destroy( &mutex->mutex );
S}
S
Sstatic int threading_mutex_lock_pthread( mbedtls_threading_mutex_t *mutex )
S{
S    if( mutex == NULL || ! mutex->is_valid )
S        return( MBEDTLS_ERR_THREADING_BAD_INPUT_DATA );
S
S    if( pthread_mutex_lock( &mutex->mutex ) != 0 )
S        return( MBEDTLS_ERR_THREADING_MUTEX_ERROR );
S
S    return( 0 );
S}
S
Sstatic int threading_mutex_unlock_pthread( mbedtls_threading_mutex_t *mutex )
S{
S    if( mutex == NULL || ! mutex->is_valid )
S        return( MBEDTLS_ERR_THREADING_BAD_INPUT_DATA );
S
S    if( pthread_mutex_unlock( &mutex->mutex ) != 0 )
S        return( MBEDTLS_ERR_THREADING_MUTEX_ERROR );
S
S    return( 0 );
S}
