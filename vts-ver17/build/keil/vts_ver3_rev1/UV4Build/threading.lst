S/*
S * Set functions pointers and initialize global mutexes
S */
Svoid mbedtls_threading_set_alt( void (*mutex_init)( mbedtls_threading_mutex_t * ),
S                       void (*mutex_free)( mbedtls_threading_mutex_t * ),
S                       int (*mutex_lock)( mbedtls_threading_mutex_t * ),
S                       int (*mutex_unlock)( mbedtls_threading_mutex_t * ) )
S{
S    mbedtls_mutex_init = mutex_init;
