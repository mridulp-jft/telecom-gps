S/*
S * Check a public RSA key
S */
Sint mbedtls_rsa_check_pubkey( const mbedtls_rsa_context *ctx )
S{
S    if( !ctx->N.p || !ctx->E.p )
S        return( MBEDTLS_ERR_RSA_KEY_CHECK_FAILED );
S
S    if( ( ctx->N.p[0] & 1 ) == 0 ||
S        ( ctx->E.p[0] & 1 ) == 0 )
S        return( MBEDTLS_ERR_RSA_KEY_CHECK_FAILED );
S
S    if( mbedtls_mpi_bitlen( &ctx->N ) < 128 ||
S        mbedtls_mpi_bitlen( &ctx->N ) > MBEDTLS_MPI_MAX_BITS )
S        return( MBEDTLS_ERR_RSA_KEY_CHECK_FAILED );
S
S    if( mbedtls_mpi_bitlen( &ctx->E ) < 2 ||
S        mbedtls_mpi_cmp_mpi( &ctx->E, &ctx->N ) >= 0 )
S        return( MBEDTLS_ERR_RSA_KEY_CHECK_FAILED );
S
S    return( 0 );
S}
S
S/*
S * Check a private RSA key
S */
Sint mbedtls_rsa_check_privkey( const mbedtls_rsa_context *ctx )
S{
S    int ret;
S    mbedtls_mpi PQ, DE, P1, Q1, H, I, G, G2, L1, L2, DP, DQ, QP;
S
S    if( ( ret = mbedtls_rsa_check_pubkey( ctx ) ) != 0 )
S        return( ret );
S
S    if( !ctx->P.p || !ctx->Q.p || !ctx->D.p )
S        return( MBEDTLS_ERR_RSA_KEY_CHECK_FAILED );
S
S    mbedtls_mpi_init( &PQ ); mbedtls_mpi_init( &DE ); mbedtls_mpi_init( &P1 ); mbedtls_mpi_init( &Q1 );
S    mbedtls_mpi_init( &H  ); mbedtls_mpi_init( &I  ); mbedtls_mpi_init( &G  ); mbedtls_mpi_init( &G2 );
S    mbedtls_mpi_init( &L1 ); mbedtls_mpi_init( &L2 ); mbedtls_mpi_init( &DP ); mbedtls_mpi_init( &DQ );
S    mbedtls_mpi_init( &QP );
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &PQ, &ctx->P, &ctx->Q ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &DE, &ctx->D, &ctx->E ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_sub_int( &P1, &ctx->P, 1 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_sub_int( &Q1, &ctx->Q, 1 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &H, &P1, &Q1 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_gcd( &G, &ctx->E, &H  ) );
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_gcd( &G2, &P1, &Q1 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_div_mpi( &L1, &L2, &H, &G2 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &I, &DE, &L1  ) );
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &DP, &ctx->D, &P1 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &DQ, &ctx->D, &Q1 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_inv_mod( &QP, &ctx->Q, &ctx->P ) );
S    /*
S     * Check for a valid PKCS1v2 private key
S     */
S    if( mbedtls_mpi_cmp_mpi( &PQ, &ctx->N ) != 0 ||
S        mbedtls_mpi_cmp_mpi( &DP, &ctx->DP ) != 0 ||
S        mbedtls_mpi_cmp_mpi( &DQ, &ctx->DQ ) != 0 ||
S        mbedtls_mpi_cmp_mpi( &QP, &ctx->QP ) != 0 ||
S        mbedtls_mpi_cmp_int( &L2, 0 ) != 0 ||
S        mbedtls_mpi_cmp_int( &I, 1 ) != 0 ||
S        mbedtls_mpi_cmp_int( &G, 1 ) != 0 )
S    {
S        ret = MBEDTLS_ERR_RSA_KEY_CHECK_FAILED;
S    }
S
Scleanup:
S    mbedtls_mpi_free( &PQ ); mbedtls_mpi_free( &DE ); mbedtls_mpi_free( &P1 ); mbedtls_mpi_free( &Q1 );
S    mbedtls_mpi_free( &H  ); mbedtls_mpi_free( &I  ); mbedtls_mpi_free( &G  ); mbedtls_mpi_free( &G2 );
S    mbedtls_mpi_free( &L1 ); mbedtls_mpi_free( &L2 ); mbedtls_mpi_free( &DP ); mbedtls_mpi_free( &DQ );
S    mbedtls_mpi_free( &QP );
S
S    if( ret == MBEDTLS_ERR_RSA_KEY_CHECK_FAILED )
S        return( ret );
S
S    if( ret != 0 )
S        return( MBEDTLS_ERR_RSA_KEY_CHECK_FAILED + ret );
S
S    return( 0 );
S}
S
S/*
S * Check if contexts holding a public and private key match
S */
Sint mbedtls_rsa_check_pub_priv( const mbedtls_rsa_context *pub, const mbedtls_rsa_context *prv )
S{
S    if( mbedtls_rsa_check_pubkey( pub ) != 0 ||
S        mbedtls_rsa_check_privkey( prv ) != 0 )
S    {
S        return( MBEDTLS_ERR_RSA_KEY_CHECK_FAILED );
S    }
S
S    if( mbedtls_mpi_cmp_mpi( &pub->N, &prv->N ) != 0 ||
S        mbedtls_mpi_cmp_mpi( &pub->E, &prv->E ) != 0 )
S    {
S        return( MBEDTLS_ERR_RSA_KEY_CHECK_FAILED );
S    }
S
S    return( 0 );
S}
S
S/*
S * Do an RSA public key operation
S */
Sint mbedtls_rsa_public( mbedtls_rsa_context *ctx,
S                const unsigned char *input,
S                unsigned char *output )
S{
S    int ret;
S    size_t olen;
S    mbedtls_mpi T;
S
S    mbedtls_mpi_init( &T );
S
S#if defined(MBEDTLS_THREADING_C)
S    if( ( ret = mbedtls_mutex_lock( &ctx->mutex ) ) != 0 )
S        return( ret );
S#endif
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_binary( &T, input, ctx->len ) );
S
S    if( mbedtls_mpi_cmp_mpi( &T, &ctx->N ) >= 0 )
S    {
S        ret = MBEDTLS_ERR_MPI_BAD_INPUT_DATA;
S        goto cleanup;
S    }
S
S    olen = ctx->len;
S    MBEDTLS_MPI_CHK( mbedtls_mpi_exp_mod( &T, &T, &ctx->E, &ctx->N, &ctx->RN ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_write_binary( &T, output, olen ) );
S
Scleanup:
S#if defined(MBEDTLS_THREADING_C)
S    if( mbedtls_mutex_unlock( &ctx->mutex ) != 0 )
S        return( MBEDTLS_ERR_THREADING_MUTEX_ERROR );
S#endif
S
S    mbedtls_mpi_free( &T );
S
S    if( ret != 0 )
S        return( MBEDTLS_ERR_RSA_PUBLIC_FAILED + ret );
S
S    return( 0 );
S}
