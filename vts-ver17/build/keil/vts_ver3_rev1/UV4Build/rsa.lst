S/*
S * Copy the components of an RSA key
S */
Sint mbedtls_rsa_copy( mbedtls_rsa_context *dst, const mbedtls_rsa_context *src )
S{
S    int ret;
S
S    dst->ver = src->ver;
S    dst->len = src->len;
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->N, &src->N ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->E, &src->E ) );
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->D, &src->D ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->P, &src->P ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->Q, &src->Q ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->DP, &src->DP ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->DQ, &src->DQ ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->QP, &src->QP ) );
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->RN, &src->RN ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->RP, &src->RP ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->RQ, &src->RQ ) );
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->Vi, &src->Vi ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_copy( &dst->Vf, &src->Vf ) );
S
S    dst->padding = src->padding;
S    dst->hash_id = src->hash_id;
S
Scleanup:
S    if( ret != 0 )
S        mbedtls_rsa_free( dst );
S
S    return( ret );
S}
S
S/*
S * Free the components of an RSA key
S */
Svoid mbedtls_rsa_free( mbedtls_rsa_context *ctx )
S{
S    mbedtls_mpi_free( &ctx->Vi ); mbedtls_mpi_free( &ctx->Vf );
S    mbedtls_mpi_free( &ctx->RQ ); mbedtls_mpi_free( &ctx->RP ); mbedtls_mpi_free( &ctx->RN );
S    mbedtls_mpi_free( &ctx->QP ); mbedtls_mpi_free( &ctx->DQ ); mbedtls_mpi_free( &ctx->DP );
S    mbedtls_mpi_free( &ctx->Q  ); mbedtls_mpi_free( &ctx->P  ); mbedtls_mpi_free( &ctx->D );
S    mbedtls_mpi_free( &ctx->E  ); mbedtls_mpi_free( &ctx->N  );
S
S#if defined(MBEDTLS_THREADING_C)
S    mbedtls_mutex_free( &ctx->mutex );
S#endif
S}
