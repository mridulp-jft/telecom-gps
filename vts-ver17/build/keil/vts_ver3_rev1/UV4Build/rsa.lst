S    if( siglen < 16 || siglen > sizeof( buf ) )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    ret = ( mode == MBEDTLS_RSA_PUBLIC )
S          ? mbedtls_rsa_public(  ctx, sig, buf )
S          : mbedtls_rsa_private( ctx, f_rng, p_rng, sig, buf );
S
S    if( ret != 0 )
S        return( ret );
S
S    p = buf;
S
S    if( *p++ != 0 || *p++ != MBEDTLS_RSA_SIGN )
S        return( MBEDTLS_ERR_RSA_INVALID_PADDING );
S
S    while( *p != 0 )
S    {
S        if( p >= buf + siglen - 1 || *p != 0xFF )
S            return( MBEDTLS_ERR_RSA_INVALID_PADDING );
S        p++;
S    }
S    p++;
S
S    len = siglen - ( p - buf );
S
S    if( len == hashlen && md_alg == MBEDTLS_MD_NONE )
S    {
S        if( memcmp( p, hash, hashlen ) == 0 )
S            return( 0 );
S        else
S            return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S    }
S
S    md_info = mbedtls_md_info_from_type( md_alg );
S    if( md_info == NULL )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S    hashlen = mbedtls_md_get_size( md_info );
S
S    end = p + len;
S
S    // Parse the ASN.1 structure inside the PKCS#1 v1.5 structure
S    //
S    if( ( ret = mbedtls_asn1_get_tag( &p, end, &asn1_len,
S            MBEDTLS_ASN1_CONSTRUCTED | MBEDTLS_ASN1_SEQUENCE ) ) != 0 )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    if( asn1_len + 2 != len )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    if( ( ret = mbedtls_asn1_get_tag( &p, end, &asn1_len,
S            MBEDTLS_ASN1_CONSTRUCTED | MBEDTLS_ASN1_SEQUENCE ) ) != 0 )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    if( asn1_len + 6 + hashlen != len )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    if( ( ret = mbedtls_asn1_get_tag( &p, end, &oid.len, MBEDTLS_ASN1_OID ) ) != 0 )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    oid.p = p;
S    p += oid.len;
S
S    if( mbedtls_oid_get_md_alg( &oid, &msg_md_alg ) != 0 )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    if( md_alg != msg_md_alg )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    /*
S     * assume the algorithm parameters must be NULL
S     */
S    if( ( ret = mbedtls_asn1_get_tag( &p, end, &asn1_len, MBEDTLS_ASN1_NULL ) ) != 0 )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    if( ( ret = mbedtls_asn1_get_tag( &p, end, &asn1_len, MBEDTLS_ASN1_OCTET_STRING ) ) != 0 )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    if( asn1_len != hashlen )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    if( memcmp( p, hash, hashlen ) != 0 )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    p += hashlen;
S
S    if( p != end )
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S
S    return( 0 );
S}
S#endif /* MBEDTLS_PKCS1_V15 */
S
S/*
S * Do an RSA operation and check the message digest
S */
Sint mbedtls_rsa_pkcs1_verify( mbedtls_rsa_context *ctx,
S                      int (*f_rng)(void *, unsigned char *, size_t),
S                      void *p_rng,
S                      int mode,
S                      mbedtls_md_type_t md_alg,
S                      unsigned int hashlen,
S                      const unsigned char *hash,
S                      const unsigned char *sig )
S{
S    switch( ctx->padding )
S    {
S#if defined(MBEDTLS_PKCS1_V15)
S        case MBEDTLS_RSA_PKCS_V15:
S            return mbedtls_rsa_rsassa_pkcs1_v15_verify( ctx, f_rng, p_rng, mode, md_alg,
S                                                hashlen, hash, sig );
S#endif
S
S#if defined(MBEDTLS_PKCS1_V21)
S        case MBEDTLS_RSA_PKCS_V21:
S            return mbedtls_rsa_rsassa_pss_verify( ctx, f_rng, p_rng, mode, md_alg,
S                                          hashlen, hash, sig );
S#endif
S
S        default:
S            return( MBEDTLS_ERR_RSA_INVALID_PADDING );
S    }
S}
