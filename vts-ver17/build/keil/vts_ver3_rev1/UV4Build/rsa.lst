S/*
S * Initialize an RSA context
S */
Svoid mbedtls_rsa_init( mbedtls_rsa_context *ctx,
S               int padding,
S               int hash_id )
S{
S    memset( ctx, 0, sizeof( mbedtls_rsa_context ) );
S
S    mbedtls_rsa_set_padding( ctx, padding, hash_id );
S
S#if defined(MBEDTLS_THREADING_C)
S    mbedtls_mutex_init( &ctx->mutex );
S#endif
S}
S
S/*
S * Set padding for an existing RSA context
S */
Svoid mbedtls_rsa_set_padding( mbedtls_rsa_context *ctx, int padding, int hash_id )
S{
S    ctx->padding = padding;
S    ctx->hash_id = hash_id;
S}
