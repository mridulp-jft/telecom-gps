S#if defined(MBEDTLS_PKCS1_V15)
S/*
S * Implementation of the PKCS#1 v2.1 RSASSA-PKCS1-V1_5-SIGN function
S */
S/*
S * Do an RSA operation to sign the message digest
S */
Sint mbedtls_rsa_rsassa_pkcs1_v15_sign( mbedtls_rsa_context *ctx,
S                               int (*f_rng)(void *, unsigned char *, size_t),
S                               void *p_rng,
S                               int mode,
S                               mbedtls_md_type_t md_alg,
S                               unsigned int hashlen,
S                               const unsigned char *hash,
S                               unsigned char *sig )
S{
S    size_t nb_pad, olen, oid_size = 0;
S    unsigned char *p = sig;
S    const char *oid = NULL;
S    unsigned char *sig_try = NULL, *verif = NULL;
S    size_t i;
S    unsigned char diff;
S    volatile unsigned char diff_no_optimize;
S    int ret;
S
S    if( mode == MBEDTLS_RSA_PRIVATE && ctx->padding != MBEDTLS_RSA_PKCS_V15 )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    olen = ctx->len;
S    nb_pad = olen - 3;
S
S    if( md_alg != MBEDTLS_MD_NONE )
S    {
S        const mbedtls_md_info_t *md_info = mbedtls_md_info_from_type( md_alg );
S        if( md_info == NULL )
S            return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S        if( mbedtls_oid_get_oid_by_md( md_alg, &oid, &oid_size ) != 0 )
S            return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S        nb_pad -= 10 + oid_size;
S
S        hashlen = mbedtls_md_get_size( md_info );
S    }
S
S    nb_pad -= hashlen;
S
S    if( ( nb_pad < 8 ) || ( nb_pad > olen ) )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    *p++ = 0;
S    *p++ = MBEDTLS_RSA_SIGN;
S    memset( p, 0xFF, nb_pad );
S    p += nb_pad;
S    *p++ = 0;
S
S    if( md_alg == MBEDTLS_MD_NONE )
S    {
S        memcpy( p, hash, hashlen );
S    }
S    else
S    {
S        /*
S         * DigestInfo ::= SEQUENCE {
S         *   digestAlgorithm DigestAlgorithmIdentifier,
S         *   digest Digest }
S         *
S         * DigestAlgorithmIdentifier ::= AlgorithmIdentifier
S         *
S         * Digest ::= OCTET STRING
S         */
S        *p++ = MBEDTLS_ASN1_SEQUENCE | MBEDTLS_ASN1_CONSTRUCTED;
S        *p++ = (unsigned char) ( 0x08 + oid_size + hashlen );
S        *p++ = MBEDTLS_ASN1_SEQUENCE | MBEDTLS_ASN1_CONSTRUCTED;
S        *p++ = (unsigned char) ( 0x04 + oid_size );
S        *p++ = MBEDTLS_ASN1_OID;
S        *p++ = oid_size & 0xFF;
S        memcpy( p, oid, oid_size );
S        p += oid_size;
S        *p++ = MBEDTLS_ASN1_NULL;
S        *p++ = 0x00;
S        *p++ = MBEDTLS_ASN1_OCTET_STRING;
S        *p++ = hashlen;
S        memcpy( p, hash, hashlen );
S    }
S
S    if( mode == MBEDTLS_RSA_PUBLIC )
S        return( mbedtls_rsa_public(  ctx, sig, sig ) );
S
S    /*
S     * In order to prevent Lenstra's attack, make the signature in a
S     * temporary buffer and check it before returning it.
S     */
S    sig_try = mbedtls_calloc( 1, ctx->len );
S    if( sig_try == NULL )
S        return( MBEDTLS_ERR_MPI_ALLOC_FAILED );
S
S    verif   = mbedtls_calloc( 1, ctx->len );
S    if( verif == NULL )
S    {
S        mbedtls_free( sig_try );
S        return( MBEDTLS_ERR_MPI_ALLOC_FAILED );
S    }
S
S    MBEDTLS_MPI_CHK( mbedtls_rsa_private( ctx, f_rng, p_rng, sig, sig_try ) );
S    MBEDTLS_MPI_CHK( mbedtls_rsa_public( ctx, sig_try, verif ) );
S
S    /* Compare in constant time just in case */
S    for( diff = 0, i = 0; i < ctx->len; i++ )
S        diff |= verif[i] ^ sig[i];
S    diff_no_optimize = diff;
S
S    if( diff_no_optimize != 0 )
S    {
S        ret = MBEDTLS_ERR_RSA_PRIVATE_FAILED;
S        goto cleanup;
S    }
S
S    memcpy( sig, sig_try, ctx->len );
S
Scleanup:
S    mbedtls_free( sig_try );
S    mbedtls_free( verif );
S
S    return( ret );
S}
S#endif /* MBEDTLS_PKCS1_V15 */
