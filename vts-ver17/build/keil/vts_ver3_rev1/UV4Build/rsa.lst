S    //
S    mbedtls_md_starts( &md_ctx );
S    mbedtls_md_update( &md_ctx, zeros, 8 );
S    mbedtls_md_update( &md_ctx, hash, hashlen );
S    mbedtls_md_update( &md_ctx, p, slen );
S    mbedtls_md_finish( &md_ctx, result );
S
S    mbedtls_md_free( &md_ctx );
S
S    if( memcmp( p + slen, result, hlen ) == 0 )
S        return( 0 );
S    else
S        return( MBEDTLS_ERR_RSA_VERIFY_FAILED );
S}
