S/*
S * Checkup routine
S */
Sint mbedtls_rsa_self_test( int verbose )
S{
S    int ret = 0;
S#if defined(MBEDTLS_PKCS1_V15)
S    size_t len;
S    mbedtls_rsa_context rsa;
S    unsigned char rsa_plaintext[PT_LEN];
S    unsigned char rsa_decrypted[PT_LEN];
S    unsigned char rsa_ciphertext[KEY_LEN];
S#if defined(MBEDTLS_SHA1_C)
S    unsigned char sha1sum[20];
S#endif
S
S    mbedtls_rsa_init( &rsa, MBEDTLS_RSA_PKCS_V15, 0 );
S
S    rsa.len = KEY_LEN;
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_string( &rsa.N , 16, RSA_N  ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_string( &rsa.E , 16, RSA_E  ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_string( &rsa.D , 16, RSA_D  ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_string( &rsa.P , 16, RSA_P  ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_string( &rsa.Q , 16, RSA_Q  ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_string( &rsa.DP, 16, RSA_DP ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_string( &rsa.DQ, 16, RSA_DQ ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_string( &rsa.QP, 16, RSA_QP ) );
S
S    if( verbose != 0 )
S        mbedtls_printf( "  RSA key validation: " );
S
S    if( mbedtls_rsa_check_pubkey(  &rsa ) != 0 ||
S        mbedtls_rsa_check_privkey( &rsa ) != 0 )
S    {
S        if( verbose != 0 )
S            mbedtls_printf( "failed\n" );
S
S        return( 1 );
S    }
S
S    if( verbose != 0 )
S        mbedtls_printf( "passed\n  PKCS#1 encryption : " );
S
S    memcpy( rsa_plaintext, RSA_PT, PT_LEN );
S
S    if( mbedtls_rsa_pkcs1_encrypt( &rsa, myrand, NULL, MBEDTLS_RSA_PUBLIC, PT_LEN,
S                           rsa_plaintext, rsa_ciphertext ) != 0 )
S    {
S        if( verbose != 0 )
S            mbedtls_printf( "failed\n" );
S
S        return( 1 );
S    }
S
S    if( verbose != 0 )
S        mbedtls_printf( "passed\n  PKCS#1 decryption : " );
S
S    if( mbedtls_rsa_pkcs1_decrypt( &rsa, myrand, NULL, MBEDTLS_RSA_PRIVATE, &len,
S                           rsa_ciphertext, rsa_decrypted,
S                           sizeof(rsa_decrypted) ) != 0 )
S    {
S        if( verbose != 0 )
S            mbedtls_printf( "failed\n" );
S
S        return( 1 );
S    }
S
S    if( memcmp( rsa_decrypted, rsa_plaintext, len ) != 0 )
S    {
S        if( verbose != 0 )
S            mbedtls_printf( "failed\n" );
S
S        return( 1 );
S    }
S
S    if( verbose != 0 )
S        mbedtls_printf( "passed\n" );
S
S#if defined(MBEDTLS_SHA1_C)
S    if( verbose != 0 )
S        mbedtls_printf( "PKCS#1 data sign  : " );
S
S    mbedtls_sha1( rsa_plaintext, PT_LEN, sha1sum );
S
S    if( mbedtls_rsa_pkcs1_sign( &rsa, myrand, NULL, MBEDTLS_RSA_PRIVATE, MBEDTLS_MD_SHA1, 0,
S                        sha1sum, rsa_ciphertext ) != 0 )
S    {
S        if( verbose != 0 )
S            mbedtls_printf( "failed\n" );
S
S        return( 1 );
S    }
S
S    if( verbose != 0 )
S        mbedtls_printf( "passed\n  PKCS#1 sig. verify: " );
S
S    if( mbedtls_rsa_pkcs1_verify( &rsa, NULL, NULL, MBEDTLS_RSA_PUBLIC, MBEDTLS_MD_SHA1, 0,
S                          sha1sum, rsa_ciphertext ) != 0 )
S    {
S        if( verbose != 0 )
S            mbedtls_printf( "failed\n" );
S
S        return( 1 );
S    }
S
S    if( verbose != 0 )
S        mbedtls_printf( "passed\n" );
S#endif /* MBEDTLS_SHA1_C */
S
S    if( verbose != 0 )
S        mbedtls_printf( "\n" );
S
Scleanup:
S    mbedtls_rsa_free( &rsa );
S#else /* MBEDTLS_PKCS1_V15 */
S    ((void) verbose);
S#endif /* MBEDTLS_PKCS1_V15 */
S    return( ret );
S}
