S#if defined(MBEDTLS_PKCS1_V21)
S/*
S * Implementation of the PKCS#1 v2.1 RSASSA-PSS-VERIFY function
S */
Sint mbedtls_rsa_rsassa_pss_verify_ext( mbedtls_rsa_context *ctx,
S                               int (*f_rng)(void *, unsigned char *, size_t),
S                               void *p_rng,
S                               int mode,
S                               mbedtls_md_type_t md_alg,
S                               unsigned int hashlen,
S                               const unsigned char *hash,
S                               mbedtls_md_type_t mgf1_hash_id,
S                               int expected_salt_len,
S                               const unsigned char *sig )
S{
S    int ret;
S    size_t siglen;
S    unsigned char *p;
S    unsigned char buf[MBEDTLS_MPI_MAX_SIZE];
S    unsigned char result[MBEDTLS_MD_MAX_SIZE];
S    unsigned char zeros[8];
S    unsigned int hlen;
S    size_t slen, msb;
S    const mbedtls_md_info_t *md_info;
S    mbedtls_md_context_t md_ctx;
S
S    if( mode == MBEDTLS_RSA_PRIVATE && ctx->padding != MBEDTLS_RSA_PKCS_V21 )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    siglen = ctx->len;
S
S    if( siglen < 16 || siglen > sizeof( buf ) )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    ret = ( mode == MBEDTLS_RSA_PUBLIC )
S          ? mbedtls_rsa_public(  ctx, sig, buf )
S          : mbedtls_rsa_private( ctx, f_rng, p_rng, sig, buf );
S
S    if( ret != 0 )
S        return( ret );
S
S    p = buf;
S
S    if( buf[siglen - 1] != 0xBC )
S        return( MBEDTLS_ERR_RSA_INVALID_PADDING );
S
S    if( md_alg != MBEDTLS_MD_NONE )
S    {
S        // Gather length of hash to sign
S        //
S        md_info = mbedtls_md_info_from_type( md_alg );
S        if( md_info == NULL )
S            return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S        hashlen = mbedtls_md_get_size( md_info );
S    }
S
S    md_info = mbedtls_md_info_from_type( mgf1_hash_id );
S    if( md_info == NULL )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    hlen = mbedtls_md_get_size( md_info );
S    slen = siglen - hlen - 1; /* Currently length of salt + padding */
S
S    memset( zeros, 0, 8 );
S
S    // Note: EMSA-PSS verification is over the length of N - 1 bits
S    //
S    msb = mbedtls_mpi_bitlen( &ctx->N ) - 1;
S
S    // Compensate for boundary condition when applying mask
S    //
S    if( msb % 8 == 0 )
S    {
S        p++;
S        siglen -= 1;
S    }
S    if( buf[0] >> ( 8 - siglen * 8 + msb ) )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    mbedtls_md_init( &md_ctx );
S    mbedtls_md_setup( &md_ctx, md_info, 0 );
S
S    mgf_mask( p, siglen - hlen - 1, p + siglen - hlen - 1, hlen, &md_ctx );
S
S    buf[0] &= 0xFF >> ( siglen * 8 - msb );
S
S    while( p < buf + siglen && *p == 0 )
S        p++;
S
S    if( p == buf + siglen ||
S        *p++ != 0x01 )
S    {
S        mbedtls_md_free( &md_ctx );
S        return( MBEDTLS_ERR_RSA_INVALID_PADDING );
