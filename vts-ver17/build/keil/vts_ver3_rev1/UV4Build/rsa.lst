S/*
S * Generate an RSA keypair
S */
Sint mbedtls_rsa_gen_key( mbedtls_rsa_context *ctx,
S                 int (*f_rng)(void *, unsigned char *, size_t),
S                 void *p_rng,
S                 unsigned int nbits, int exponent )
S{
S    int ret;
S    mbedtls_mpi P1, Q1, H, G;
S
S    if( f_rng == NULL || nbits < 128 || exponent < 3 )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    mbedtls_mpi_init( &P1 ); mbedtls_mpi_init( &Q1 ); mbedtls_mpi_init( &H ); mbedtls_mpi_init( &G );
S
S    /*
S     * find primes P and Q with Q < P so that:
S     * GCD( E, (P-1)*(Q-1) ) == 1
S     */
S    MBEDTLS_MPI_CHK( mbedtls_mpi_lset( &ctx->E, exponent ) );
S
S    do
S    {
S        MBEDTLS_MPI_CHK( mbedtls_mpi_gen_prime( &ctx->P, ( nbits + 1 ) >> 1, 0,
S                                f_rng, p_rng ) );
S
S        MBEDTLS_MPI_CHK( mbedtls_mpi_gen_prime( &ctx->Q, ( nbits + 1 ) >> 1, 0,
S                                f_rng, p_rng ) );
S
S        if( mbedtls_mpi_cmp_mpi( &ctx->P, &ctx->Q ) < 0 )
S            mbedtls_mpi_swap( &ctx->P, &ctx->Q );
S
S        if( mbedtls_mpi_cmp_mpi( &ctx->P, &ctx->Q ) == 0 )
S            continue;
S
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &ctx->N, &ctx->P, &ctx->Q ) );
S        if( mbedtls_mpi_bitlen( &ctx->N ) != nbits )
S            continue;
S
S        MBEDTLS_MPI_CHK( mbedtls_mpi_sub_int( &P1, &ctx->P, 1 ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_sub_int( &Q1, &ctx->Q, 1 ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &H, &P1, &Q1 ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_gcd( &G, &ctx->E, &H  ) );
S    }
S    while( mbedtls_mpi_cmp_int( &G, 1 ) != 0 );
S
S    /*
S     * D  = E^-1 mod ((P-1)*(Q-1))
S     * DP = D mod (P - 1)
S     * DQ = D mod (Q - 1)
S     * QP = Q^-1 mod P
S     */
S    MBEDTLS_MPI_CHK( mbedtls_mpi_inv_mod( &ctx->D , &ctx->E, &H  ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &ctx->DP, &ctx->D, &P1 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &ctx->DQ, &ctx->D, &Q1 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_inv_mod( &ctx->QP, &ctx->Q, &ctx->P ) );
S
S    ctx->len = ( mbedtls_mpi_bitlen( &ctx->N ) + 7 ) >> 3;
S
Scleanup:
S
S    mbedtls_mpi_free( &P1 ); mbedtls_mpi_free( &Q1 ); mbedtls_mpi_free( &H ); mbedtls_mpi_free( &G );
S
S    if( ret != 0 )
S    {
S        mbedtls_rsa_free( ctx );
S        return( MBEDTLS_ERR_RSA_KEY_GEN_FAILED + ret );
S    }
S
S    return( 0 );
S}
S
S#endif /* MBEDTLS_GENPRIME */
