S/*
S * Generate or update blinding values, see section 10 of:
S *  KOCHER, Paul C. Timing attacks on implementations of Diffie-Hellman, RSA,
S *  DSS, and other systems. In : Advances in Cryptology-CRYPTO'96. Springer
S *  Berlin Heidelberg, 1996. p. 104-113.
S */
Sstatic int rsa_prepare_blinding( mbedtls_rsa_context *ctx,
S                 int (*f_rng)(void *, unsigned char *, size_t), void *p_rng )
S{
S    int ret, count = 0;
S
S    if( ctx->Vf.p != NULL )
S    {
S        /* We already have blinding values, just update them by squaring */
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &ctx->Vi, &ctx->Vi, &ctx->Vi ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &ctx->Vi, &ctx->Vi, &ctx->N ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &ctx->Vf, &ctx->Vf, &ctx->Vf ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &ctx->Vf, &ctx->Vf, &ctx->N ) );
S
S        goto cleanup;
S    }
S
S    /* Unblinding value: Vf = random number, invertible mod N */
S    do {
S        if( count++ > 10 )
S            return( MBEDTLS_ERR_RSA_RNG_FAILED );
S
S        MBEDTLS_MPI_CHK( mbedtls_mpi_fill_random( &ctx->Vf, ctx->len - 1, f_rng, p_rng ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_gcd( &ctx->Vi, &ctx->Vf, &ctx->N ) );
S    } while( mbedtls_mpi_cmp_int( &ctx->Vi, 1 ) != 0 );
S
S    /* Blinding value: Vi =  Vf^(-e) mod N */
S    MBEDTLS_MPI_CHK( mbedtls_mpi_inv_mod( &ctx->Vi, &ctx->Vf, &ctx->N ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_exp_mod( &ctx->Vi, &ctx->Vi, &ctx->E, &ctx->N, &ctx->RN ) );
S
S
Scleanup:
S    return( ret );
S}
S
S/*
S * Do an RSA private key operation
S */
Sint mbedtls_rsa_private( mbedtls_rsa_context *ctx,
S                 int (*f_rng)(void *, unsigned char *, size_t),
S                 void *p_rng,
S                 const unsigned char *input,
S                 unsigned char *output )
S{
S    int ret;
S    size_t olen;
S    mbedtls_mpi T, T1, T2;
S
S    /* Make sure we have private key info, prevent possible misuse */
S    if( ctx->P.p == NULL || ctx->Q.p == NULL || ctx->D.p == NULL )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    mbedtls_mpi_init( &T ); mbedtls_mpi_init( &T1 ); mbedtls_mpi_init( &T2 );
S
S#if defined(MBEDTLS_THREADING_C)
S    if( ( ret = mbedtls_mutex_lock( &ctx->mutex ) ) != 0 )
S        return( ret );
S#endif
S
S    MBEDTLS_MPI_CHK( mbedtls_mpi_read_binary( &T, input, ctx->len ) );
S    if( mbedtls_mpi_cmp_mpi( &T, &ctx->N ) >= 0 )
S    {
S        ret = MBEDTLS_ERR_MPI_BAD_INPUT_DATA;
S        goto cleanup;
S    }
S
S    if( f_rng != NULL )
S    {
S        /*
S         * Blinding
S         * T = T * Vi mod N
S         */
S        MBEDTLS_MPI_CHK( rsa_prepare_blinding( ctx, f_rng, p_rng ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &T, &T, &ctx->Vi ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &T, &T, &ctx->N ) );
S    }
S
S#if defined(MBEDTLS_RSA_NO_CRT)
S    MBEDTLS_MPI_CHK( mbedtls_mpi_exp_mod( &T, &T, &ctx->D, &ctx->N, &ctx->RN ) );
S#else
S    /*
S     * faster decryption using the CRT
S     *
S     * T1 = input ^ dP mod P
S     * T2 = input ^ dQ mod Q
S     */
S    MBEDTLS_MPI_CHK( mbedtls_mpi_exp_mod( &T1, &T, &ctx->DP, &ctx->P, &ctx->RP ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_exp_mod( &T2, &T, &ctx->DQ, &ctx->Q, &ctx->RQ ) );
S
S    /*
S     * T = (T1 - T2) * (Q^-1 mod P) mod P
S     */
S    MBEDTLS_MPI_CHK( mbedtls_mpi_sub_mpi( &T, &T1, &T2 ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &T1, &T, &ctx->QP ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &T, &T1, &ctx->P ) );
S
S    /*
S     * T = T2 + T * Q
S     */
S    MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &T1, &T, &ctx->Q ) );
S    MBEDTLS_MPI_CHK( mbedtls_mpi_add_mpi( &T, &T2, &T1 ) );
S#endif /* MBEDTLS_RSA_NO_CRT */
S
S    if( f_rng != NULL )
S    {
S        /*
S         * Unblind
S         * T = T * Vf mod N
S         */
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mul_mpi( &T, &T, &ctx->Vf ) );
S        MBEDTLS_MPI_CHK( mbedtls_mpi_mod_mpi( &T, &T, &ctx->N ) );
S    }
S
S    olen = ctx->len;
S    MBEDTLS_MPI_CHK( mbedtls_mpi_write_binary( &T, output, olen ) );
S
Scleanup:
S#if defined(MBEDTLS_THREADING_C)
S    if( mbedtls_mutex_unlock( &ctx->mutex ) != 0 )
S        return( MBEDTLS_ERR_THREADING_MUTEX_ERROR );
S#endif
S
S    mbedtls_mpi_free( &T ); mbedtls_mpi_free( &T1 ); mbedtls_mpi_free( &T2 );
S
S    if( ret != 0 )
S        return( MBEDTLS_ERR_RSA_PRIVATE_FAILED + ret );
S
S    return( 0 );
S}
S
S#if defined(MBEDTLS_PKCS1_V21)
S/**
S * Generate and apply the MGF1 operation (from PKCS#1 v2.1) to a buffer.
S *
S * \param dst       buffer to mask
S * \param dlen      length of destination buffer
S * \param src       source of the mask generation
S * \param slen      length of the source buffer
S * \param md_ctx    message digest context to use
S */
Sstatic void mgf_mask( unsigned char *dst, size_t dlen, unsigned char *src,
S                      size_t slen, mbedtls_md_context_t *md_ctx )
S{
S    unsigned char mask[MBEDTLS_MD_MAX_SIZE];
S    unsigned char counter[4];
S    unsigned char *p;
S    unsigned int hlen;
S    size_t i, use_len;
S
S    memset( mask, 0, MBEDTLS_MD_MAX_SIZE );
S    memset( counter, 0, 4 );
S
S    hlen = mbedtls_md_get_size( md_ctx->md_info );
S
S    // Generate and apply dbMask
S    //
S    p = dst;
S
S    while( dlen > 0 )
S    {
S        use_len = hlen;
S        if( dlen < hlen )
S            use_len = dlen;
S
S        mbedtls_md_starts( md_ctx );
S        mbedtls_md_update( md_ctx, src, slen );
S        mbedtls_md_update( md_ctx, counter, 4 );
S        mbedtls_md_finish( md_ctx, mask );
S
S        for( i = 0; i < use_len; ++i )
S            *p++ ^= mask[i];
S
S        counter[3]++;
S
S        dlen -= use_len;
S    }
S}
S#endif /* MBEDTLS_PKCS1_V21 */
S
S#if defined(MBEDTLS_PKCS1_V21)
S/*
S * Implementation of the PKCS#1 v2.1 RSAES-OAEP-ENCRYPT function
S */
Sint mbedtls_rsa_rsaes_oaep_encrypt( mbedtls_rsa_context *ctx,
S                            int (*f_rng)(void *, unsigned char *, size_t),
S                            void *p_rng,
S                            int mode,
S                            const unsigned char *label, size_t label_len,
S                            size_t ilen,
S                            const unsigned char *input,
S                            unsigned char *output )
S{
S    size_t olen;
S    int ret;
S    unsigned char *p = output;
S    unsigned int hlen;
S    const mbedtls_md_info_t *md_info;
S    mbedtls_md_context_t md_ctx;
S
S    if( mode == MBEDTLS_RSA_PRIVATE && ctx->padding != MBEDTLS_RSA_PKCS_V21 )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    if( f_rng == NULL )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    md_info = mbedtls_md_info_from_type( (mbedtls_md_type_t) ctx->hash_id );
S    if( md_info == NULL )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    olen = ctx->len;
S    hlen = mbedtls_md_get_size( md_info );
S
S    if( olen < ilen + 2 * hlen + 2 )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    memset( output, 0, olen );
S
S    *p++ = 0;
S
S    // Generate a random octet string seed
S    //
S    if( ( ret = f_rng( p_rng, p, hlen ) ) != 0 )
S        return( MBEDTLS_ERR_RSA_RNG_FAILED + ret );
S
S    p += hlen;
S
S    // Construct DB
S    //
S    mbedtls_md( md_info, label, label_len, p );
S    p += hlen;
S    p += olen - 2 * hlen - 2 - ilen;
S    *p++ = 1;
S    memcpy( p, input, ilen );
S
S    mbedtls_md_init( &md_ctx );
S    mbedtls_md_setup( &md_ctx, md_info, 0 );
S
S    // maskedDB: Apply dbMask to DB
S    //
S    mgf_mask( output + hlen + 1, olen - hlen - 1, output + 1, hlen,
S               &md_ctx );
S
S    // maskedSeed: Apply seedMask to seed
S    //
S    mgf_mask( output + 1, hlen, output + hlen + 1, olen - hlen - 1,
S               &md_ctx );
S
S    mbedtls_md_free( &md_ctx );
S
S    return( ( mode == MBEDTLS_RSA_PUBLIC )
S            ? mbedtls_rsa_public(  ctx, output, output )
S            : mbedtls_rsa_private( ctx, f_rng, p_rng, output, output ) );
S}
S#endif /* MBEDTLS_PKCS1_V21 */
S
S#if defined(MBEDTLS_PKCS1_V15)
S/*
S * Implementation of the PKCS#1 v2.1 RSAES-PKCS1-V1_5-ENCRYPT function
S */
Sint mbedtls_rsa_rsaes_pkcs1_v15_encrypt( mbedtls_rsa_context *ctx,
S                                 int (*f_rng)(void *, unsigned char *, size_t),
S                                 void *p_rng,
S                                 int mode, size_t ilen,
S                                 const unsigned char *input,
S                                 unsigned char *output )
S{
S    size_t nb_pad, olen;
S    int ret;
S    unsigned char *p = output;
S
S    if( mode == MBEDTLS_RSA_PRIVATE && ctx->padding != MBEDTLS_RSA_PKCS_V15 )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    if( f_rng == NULL )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    olen = ctx->len;
S
S    if( olen < ilen + 11 )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    nb_pad = olen - 3 - ilen;
S
S    *p++ = 0;
S    if( mode == MBEDTLS_RSA_PUBLIC )
S    {
S        *p++ = MBEDTLS_RSA_CRYPT;
S
S        while( nb_pad-- > 0 )
S        {
S            int rng_dl = 100;
S
S            do {
S                ret = f_rng( p_rng, p, 1 );
S            } while( *p == 0 && --rng_dl && ret == 0 );
S
S            // Check if RNG failed to generate data
S            //
S            if( rng_dl == 0 || ret != 0 )
S                return( MBEDTLS_ERR_RSA_RNG_FAILED + ret );
S
S            p++;
S        }
S    }
S    else
S    {
S        *p++ = MBEDTLS_RSA_SIGN;
S
S        while( nb_pad-- > 0 )
S            *p++ = 0xFF;
S    }
S
S    *p++ = 0;
S    memcpy( p, input, ilen );
S
S    return( ( mode == MBEDTLS_RSA_PUBLIC )
S            ? mbedtls_rsa_public(  ctx, output, output )
S            : mbedtls_rsa_private( ctx, f_rng, p_rng, output, output ) );
S}
S#endif /* MBEDTLS_PKCS1_V15 */
S
S/*
S * Add the message padding, then do an RSA operation
S */
Sint mbedtls_rsa_pkcs1_encrypt( mbedtls_rsa_context *ctx,
S                       int (*f_rng)(void *, unsigned char *, size_t),
S                       void *p_rng,
S                       int mode, size_t ilen,
S                       const unsigned char *input,
S                       unsigned char *output )
S{
S    switch( ctx->padding )
S    {
S#if defined(MBEDTLS_PKCS1_V15)
S        case MBEDTLS_RSA_PKCS_V15:
S            return mbedtls_rsa_rsaes_pkcs1_v15_encrypt( ctx, f_rng, p_rng, mode, ilen,
S                                                input, output );
S#endif
S
S#if defined(MBEDTLS_PKCS1_V21)
S        case MBEDTLS_RSA_PKCS_V21:
S            return mbedtls_rsa_rsaes_oaep_encrypt( ctx, f_rng, p_rng, mode, NULL, 0,
S                                           ilen, input, output );
S#endif
S
S        default:
S            return( MBEDTLS_ERR_RSA_INVALID_PADDING );
S    }
S}
S
S#if defined(MBEDTLS_PKCS1_V21)
S/*
S * Implementation of the PKCS#1 v2.1 RSAES-OAEP-DECRYPT function
S */
Sint mbedtls_rsa_rsaes_oaep_decrypt( mbedtls_rsa_context *ctx,
S                            int (*f_rng)(void *, unsigned char *, size_t),
S                            void *p_rng,
S                            int mode,
S                            const unsigned char *label, size_t label_len,
S                            size_t *olen,
S                            const unsigned char *input,
S                            unsigned char *output,
S                            size_t output_max_len )
S{
S    int ret;
S    size_t ilen, i, pad_len;
S    unsigned char *p, bad, pad_done;
S    unsigned char buf[MBEDTLS_MPI_MAX_SIZE];
S    unsigned char lhash[MBEDTLS_MD_MAX_SIZE];
S    unsigned int hlen;
S    const mbedtls_md_info_t *md_info;
S    mbedtls_md_context_t md_ctx;
S
S    /*
S     * Parameters sanity checks
S     */
S    if( mode == MBEDTLS_RSA_PRIVATE && ctx->padding != MBEDTLS_RSA_PKCS_V21 )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    ilen = ctx->len;
S
S    if( ilen < 16 || ilen > sizeof( buf ) )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    md_info = mbedtls_md_info_from_type( (mbedtls_md_type_t) ctx->hash_id );
S    if( md_info == NULL )
S        return( MBEDTLS_ERR_RSA_BAD_INPUT_DATA );
S
S    /*
S     * RSA operation
S     */
S    ret = ( mode == MBEDTLS_RSA_PUBLIC )
S          ? mbedtls_rsa_public(  ctx, input, buf )
S          : mbedtls_rsa_private( ctx, f_rng, p_rng, input, buf );
S
S    if( ret != 0 )
S        return( ret );
S
S    /*
S     * Unmask data and generate lHash
S     */
S    hlen = mbedtls_md_get_size( md_info );
S
S    mbedtls_md_init( &md_ctx );
S    mbedtls_md_setup( &md_ctx, md_info, 0 );
S
S    /* Generate lHash */
S    mbedtls_md( md_info, label, label_len, lhash );
S
S    /* seed: Apply seedMask to maskedSeed */
S    mgf_mask( buf + 1, hlen, buf + hlen + 1, ilen - hlen - 1,
S               &md_ctx );
S
S    /* DB: Apply dbMask to maskedDB */
S    mgf_mask( buf + hlen + 1, ilen - hlen - 1, buf + 1, hlen,
S               &md_ctx );
S
S    mbedtls_md_free( &md_ctx );
S
S    /*
S     * Check contents, in "constant-time"
S     */
S    p = buf;
S    bad = 0;
S
S    bad |= *p++; /* First byte must be 0 */
S
S    p += hlen; /* Skip seed */
S
S    /* Check lHash */
S    for( i = 0; i < hlen; i++ )
S        bad |= lhash[i] ^ *p++;
S
S    /* Get zero-padding len, but always read till end of buffer
S     * (minus one, for the 01 byte) */
S    pad_len = 0;
S    pad_done = 0;
S    for( i = 0; i < ilen - 2 * hlen - 2; i++ )
S    {
S        pad_done |= p[i];
S        pad_len += ((pad_done | (unsigned char)-pad_done) >> 7) ^ 1;
S    }
S
S    p += pad_len;
S    bad |= *p++ ^ 0x01;
S
S    /*
S     * The only information "leaked" is whether the padding was correct or not
S     * (eg, no data is copied if it was not correct). This meets the
S     * recommendations in PKCS#1 v2.2: an opponent cannot distinguish between
S     * the different error conditions.
S     */
S    if( bad != 0 )
S        return( MBEDTLS_ERR_RSA_INVALID_PADDING );
S
S    if( ilen - ( p - buf ) > output_max_len )
S        return( MBEDTLS_ERR_RSA_OUTPUT_TOO_LARGE );
S
S    *olen = ilen - (p - buf);
S    memcpy( output, p, *olen );
S
S    return( 0 );
S}
S#endif /* MBEDTLS_PKCS1_V21 */
